                    <div
                      class="productView-moreItem{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}"
                      style="
                        --spacing-top: {{ spacing_top | append: 'px' }};
                        --spacing-bottom: {{ spacing_bottom | append: 'px' }}
                      "
                    >
                      <div class="productView-securityImage">
                        {%- render 'halo-trust-image', type: 'product' -%}
                      </div>
                    </div>
                  {%- endif -%}
                {%- when 'product_text' -%}
                  {%- liquid
