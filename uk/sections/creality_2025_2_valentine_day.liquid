<div class="new-year-activity font-HM_Regular">
  <div class="section-snow">
    {% assign flakes = 20 %}
    {% for i in (1..flakes) %}
      <div class="snowflake"></div>
    {% endfor %}
  </div>
  <div class="section-common section-banner" >
      <div class="wrap">
          <div class="text-box">
              <h1 class="h1-t font-HM_Bold text-left" style="color: #fff">{{ section.settings.header_title }}</h1>
              <p class="p-ds font-HM_Bold text-left" style="color: #fff">{{ section.settings.header_subTitle }}</p>
              <h3 class="font-HM_Medium text-left" style="color: #fff">{{ section.settings.header_subTitle3 }}</h3>
          </div>
          
      </div>
    <div class="countdown">
        <h3>Hurry! Time is Running Out! Ends in:</h3>
        <div class="countdown-time">
          <div class="time-item">
              <p>DAYS</p>
              <div class="days">0</div>
          </div>
          <div class="time-item">
              <p>HOURS</p>
              <div class="hours">0</div>
          </div>
          <div class="time-item">
              <p>MINS</p>
              <div class="minutes">0</div>
          </div>
          <div class="time-item">
              <p>SECS</p>
              <div class="seconds">0</div>
          </div>
      </div>
    </div>
  </div>
  <div class="section-common section-coupon">
      <div class="wrap">
          <div class="coupon-box grid lg900:grid-cols-3 grid-cols-1">
              {% for block in section.blocks %}
                {% if block.type == 'card-rule' %}
                   <div class="coupon-item">
                      <div class="coupon-head-wrap">
                          <div class="coupon-head">
                              <h4 class="coupon-head-title">{{ block.settings.card_content }}</h4>
                            {% assign currency_symbol = shop.money_format | replace: '{{amount}}', '' | strip %}
                              <p> <sup>{{ currency_symbol  }}</sup> {{ block.settings.card_price }}</p>
                          </div>
                          <div class="coupon-image">
                            {% if block.settings.card_image %}
                              {{ block.settings.card_image | image_url: width: 345 | image_tag }}
                            {% else %}
                              <img loading="lazy" src="https://cdn.shopify.com/s/files/1/0726/1221/7115/files/ecom-no-replace-yLxWCWdshAbn-christ-1.png?v=1701652005" alt="ecom-no-replace-yLxWCWdshAbn-christ-1.png?v=1701652005">
                            {% endif %}
                          </div>
                      </div>
                      
                            <div class="coupon-card">
                                <div class="coupon-card-desc">
                                    <p class="coupon-t1">{{ block.settings.coupon_title }}</p>
                                    <p class="coupon-t2">{{ block.settings.coupon_rule }}</p>
                                    <p class="coupon-t3">{{ block.settings.coupon_desc }}</p>
                                </div>
                              
                          <div class="coupon-card-code">
                              <img loading="lazy" src="https://cdn.shopify.com/s/files/1/0726/1221/7115/files/ecom-no-replace-yyi6Aj6cyKb-w.png?v=1701933473" alt="ecom-no-replace-yyi6Aj6cyKb-w.png?v=1701933473">
                          </div>
                      </div>
                  </div>
                {% endif %}
              {% endfor %}
          </div>
      </div>
  </div>

  <div class="section-common section-hotSale">
      <div class="wrap">
          <div class="hotSale-box">
              
              <h3 class="hotSale-title">{{ section.settings.hot_title }}</h3>
              <div class="hotSale-left">
