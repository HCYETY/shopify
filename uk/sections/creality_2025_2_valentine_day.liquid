  $('.product-option').on('change', function () {
    const productId = $(this).data('product-id'); // 获取当前产品 ID
    const options = getSelectedOptions(productId); // 获取当前选项值
    const variantsData = $(`#variants-${productId}`).text(); // 获取隐藏的变体数据
    const variants = JSON.parse(variantsData); // 解析变体数据

    // 匹配选中的变体
    const matchingVariant = variants.find(variant =>
      variant.options.every((option, index) => option === options[index])
    );

    // 更新价格和隐藏字段的变体 ID
    const $priceElement = $(`#price-${productId}`);
    const $comparePriceElement = $(`#compare-price-${productId}`);
    const $hiddenInput = $(`#selected-variant-${productId}`);
    const $addToCartButton = $(`#add-to-cart-${productId}`);
    if (matchingVariant) {
      // 更新现价
      $priceElement.text(formatMoney(matchingVariant.price));

      // 更新原价（如有折扣）
      if (matchingVariant.compare_at_price && matchingVariant.compare_at_price > matchingVariant.price) {
        $comparePriceElement.text(formatMoney(matchingVariant.compare_at_price)).show();
        $priceElement.addClass('discounted-price');
      } else {
        $comparePriceElement.hide();
        $priceElement.removeClass('discounted-price');
      }

      // 更新隐藏字段
      $hiddenInput.val(matchingVariant.id);
      $addToCartButton.prop('disabled', false); // 启用按钮
    } else {
      $priceElement.text('Unavailable');
      $comparePriceElement.hide();
      $hiddenInput.val(''); // 清空变体 ID，避免无效提交
      $addToCartButton.prop('disabled', true); // 禁用按钮
    }
  });

