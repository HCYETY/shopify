<script type="module">
$(function () {
  const isMobile = document.documentElement.clientWidth <= 768;
  // 监听滚动
  function fadeInListen() {
    var fadeIn = $('.container').find('.fadein');
    fadeIn.each(function () {
      let element = this;
      let {
        scrub,
        reverse,
        duration,
        actions,
        fn
      } = $(this).data()

      gsap.set(element, {
        opacity: 0,
        y: 20
      });
      let toggleActions = "play none none none";
      let start = $(this).data('start') || 70;
      duration ? '' : duration = 0.8
      let tl = gsap.to(element, {
        opacity: 1,
        y: 0,
        duration
      });
      if (reverse) {
        toggleActions = "play none none reverse"
      }

      ScrollTrigger.create({
        animation: tl,
        trigger: element,
        toggleActions,
        start: `top ${start}%`, // 调整触发点，视情况而定
        onEnter: function () {

        },
        once: true // 只触发一次
      });
    });
  }
  fadeInListen();
  // section 6 动画

  var mySwiper = new Swiper('.groups-swiper', {
    slidesPerView: 'auto',
    effect: 'fade',
    simulateTouch: false,
    fadeEffect: {
      crossFade: true
    },
    autoplay: false,
    on: {
      init: function () {
        adjustSwiperHeight();
      },
      slideChangeTransitionEnd: function () {
        // Using slideChangeTransitionEnd to ensure the class name has updated
        adjustSwiperHeight();
      }
    }
  })

  // 更新swiper 高度
  function adjustSwiperHeight() {
    var $activeSlide = $('.swiper-slide-active .msg-block');
    var slideHeight = $activeSlide.outerHeight();
    $('.groups-swiper .swiper-wrapper').height(slideHeight);
  }

  $('.tab-item').on('click', function () {
    // 移除所有选项卡的 active 类
    $('.tab-item').removeClass('active');

    // 添加 active 类到当前点击的选项卡
    $(this).addClass('active');

    // 获取对应的幻灯片索引
    var slideIndex = $(this).data('slide');

    // 切换到指定的幻灯片
    mySwiper.slideTo(slideIndex);
  });

  const countDownDate = {{ section.settings.count_down | json}}; 

  // 辅助函数：格式化时间
  function formatTime(num) {
    return num < 10 ? '0' + num : num;
  }

  function updateCountdown() {
    const now = new Date();
    const [dateStr, timeStr] = countDownDate.split(' ');
    const [year, month, day] = dateStr.split('-').map(Number);
    const [hour, minute, second] = timeStr.split(':').map(Number);
    const targetDate = new Date(year, month - 1, day, hour, minute, second);

    const timeDiff = targetDate - now;

    if (timeDiff < 1000) {
      // 倒计时结束，隐藏倒计时
      $(".count-down").hide();
      return;
    }

    // 计算剩余时间
    const totalSeconds = Math.floor(timeDiff / 1000);
    const days = Math.floor(totalSeconds / 3600 / 24);
    const hours = Math.floor(totalSeconds / 3600 % 24);
    const minutes = Math.floor(totalSeconds / 60 % 60);
    const seconds = Math.floor(totalSeconds % 60);

    // 更新页面
    $(".num").eq(0).text(formatTime(days));
    $(".num").eq(1).text(formatTime(hours));
    $(".num").eq(2).text(formatTime(minutes));
    $(".num").eq(3).text(formatTime(seconds));
  }

  // 每秒更新一次
  setInterval(updateCountdown, 1000);
  // 立即执行一次以便页面加载时就显示正确的时间
  updateCountdown();

  // 视频列表请求
  var currentPage = 1;
  var totalPages = 1;

  function fetchPage(page,query) {
    const queryParams = $.param({
      pageSize: 5,
      pageNum: page
    }); 
    $.ajax({
      url: `https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/list?${queryParams}`, // 替换为你的数据源
      method: 'POST',
      data: JSON.stringify({
        queryParam: query || ""
      }),
      dataType: 'json',
      contentType: 'application/json',
      success: function (res) {
        if(res.code == 200) {

        }
        console.log(res, "data");
        totalPages = Math.ceil(res.total / 5); // 假设每页显示 10 条数据
        updatePage(res.rows);
        updatePageInfo();
      }, 
      error: function () {
        console.log('无法获取数据');
      }
    });
  }

  // 搜索列表请求
  // 监听 Enter 键按下事件
  $('#search-input').on('keydown', function (e) {
    if (e.key === 'Enter') {
      e.preventDefault(); // 防止默认行为（例如提交表单）
      performSearch();
    }
  });

  // 监听按钮点击事件
  $('#search-button2').on('click', function () {
    performSearch();
  });

  $('#search-input2').on('keydown', function (e) {
    if (e.key === 'Enter') {
      e.preventDefault(); // 防止默认行为（例如提交表单）
      performSearch();
    }
  });

  // 监听按钮点击事件
  $('#search-button').on('click', function () {
    performSearch();
  });

  // 执行搜索的函数
  function performSearch() {
    let query;
    if(isMobile) {
      query = $('#search-input2').val().trim();
    } else {
      query = $('#search-input').val().trim();
    }
    console.log(query,'query');
    // 调用接口请求搜索数据
    currentPage = 1;
    fetchPage(currentPage,query)
  }

  // 填充列表数据
  function updatePage(data) {
    var $list = $('.youtobe-list');
    $list.empty(); // 清空现有内容

    data.forEach(item => {
      var html = `
            <div class="y-item">
                <div class="ranking">${item.rank}</div>
                <div class="video-wrap">
                    <div class="video-cover">
                    <a href="${item.videoUrl}" target="_blank" rel="noopener noreferrer">
                      <img class="cover-img" src="${item.thumbnailUrl}" alt="">
                    </a>
                    </div>
                    <div class="video-name HarmonyOS_Bold">${item.videoName}</div>
                </div>
                <div class="video-info">
                    <div class="person">
                        <div class="img-wrap">
                            <img class="avatar" src="${item.channelAvatarUrl}" alt="">
                            <div class="name HarmonyOS_Bold">${item.channelName}</div>
                        </div>
                        <div class="progress-bar">
                            <div class="progress-fill view-progress">
                                <div class="num HarmonyOS_Regular">${item.videoViews}</div>
                            </div>
                        </div>
                        <div class="progress-bar">
                            <div class="progress-fill like-progress">
                                <div class="num HarmonyOS_Regular">${item.videoLikes + item.videoComments}</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        `;
        $list.append(html);
    });
    // adjustSwiperHeight();
  }

  // 更新分页器
  function updatePageInfo() {
    $('#current-page').text(`Page ${currentPage}`);
    $('#total-pages').text(`${totalPages} pages in total`);
    $('#prev-button').prop('disabled', currentPage <= 1);
    $('#next-button').prop('disabled', currentPage >= totalPages);

    $('.current-page2').text(`Page ${currentPage}`);
    $('.total-pages2').text(`${totalPages} pages in total`);
  }

  $('#prev-button').click(function () {
    if (currentPage > 1) {
      currentPage--;
      fetchPage(currentPage);
    }
  });

  $('#next-button').click(function () {
    if (currentPage < totalPages) {
      currentPage++;
      fetchPage(currentPage);
    }
  });

  $('#prev-button2').click(function () {
    if (currentPage > 1) {
      currentPage--;
      fetchPage(currentPage);
    }
  });

  $('#next-button2').click(function () {
    if (currentPage < totalPages) {
      currentPage++;
      fetchPage(currentPage);
    }
  });

  // 初始加载第一页数据
  fetchPage(currentPage);

  // 设置总进度条
  function setProgress() {
    $.ajax({
      url: 'https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/viewsCount', // 替换为你的后台 URL
      method: 'GET',
      success: function (response) {
        var progress = response.data.viewsCount / 100000 * 100;
        progress >= 100 ? progress = 100 : "" ;
        // 更新进度条的宽度和显示的文本
        $('.inner-bar').css('width', progress + '%');
        $('.inner-bar .view .num').text(response.data.viewsCount);
      },
      error: function () {
        // 处理错误 
        // console.error('Failed to fetch progress data.');
      }
    });
  }
  setProgress()


  // 
  $(".see-btn").click(function () {
    let top;
    if (isMobile) {
      top = $(".section7").offset().top;
    } else {
      top = $(".section5").offset().top;
      $(".tab-item").eq(3).trigger("click");
