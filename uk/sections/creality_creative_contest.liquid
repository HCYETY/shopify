  $.ajax({
    url: 'https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/list?pageSize=5&pageNum=1', // 替换为你的数据源
    method: 'POST',
    data: JSON.stringify({
      queryParam: ""
    }),
    dataType: 'json',
    contentType: 'application/json',
    success: function (res) {
      if (res.code === 200) {
        // 假设数据结构是一个数组
        const videoItems = res.rows;
        const imgUrls = [
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1144.png?v=1725443954",
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1143.png?v=1725498891",
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1138.png?v=1725498891",
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1141.png?v=1725498891",
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1142.png?v=1725498891"
        ]
        
        // 清空现有的内容
        $('.t-videos').empty();
        
        // 遍历视频数据
        videoItems.forEach((video,index) => {
          // 创建视频条目的 HTML
          const videoHtml = `
            <div class="video-item">
              <img class="ranking-img" src="${imgUrls[index]}" alt="">
              <div class="p-info">
                <div class="portrait">
                  <img class="portrait-img" src="${video.channelAvatarUrl}" alt="">
                </div>
                <div class="name HarmonyOS_Bold">${video.channelName}</div>
              </div>
              <div class="video-wrap">
                <iframe src="https://www.youtube.com/embed/${extractVideoId(video.videoUrl)}" frameborder="0" 
                  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                  allowfullscreen></iframe>
              </div>
              <div class="video-info">
                <div class="item">
                  <div class="img-wrap">
                    <img class="icon" src="https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1_38645e58-24c1-4d73-b348-e4a62ce94e41.png?v=1725444892" alt="">
                  </div>
                  <div class="nums HarmonyOS_Regular">${video.videoViews}</div>
                </div>
                <div class="item">
                  <div class="img-wrap">
                    <img class="icon" src="https://cdn.shopify.com/s/files/1/0799/8714/7049/files/016aea119159532004f23b2411acf908.png?v=1725444900" alt="">
                  </div>
                  <div class="nums HarmonyOS_Regular">${video.videoComments}</div>
                </div>
              </div>
            </div>
          `;
          
          // 将 HTML 插入到容器中
          $('.t-videos').append(videoHtml);
        });
  
        // 更新页面信息
        updatePageInfo();
      } else {
        console.log('请求失败:', res.message);
      }
    },
    error: function () {
      console.log('无法获取数据');
    }
  });
  
  // 从 YouTube 视频 URL 中提取视频 ID
  function extractVideoId(url) {
    const match = url.match(/youtube\.com\/watch\?v=([^\s&]+)/) || url.match(/youtu\.be\/([^\s&]+)/);
    return match ? match[1] : '';
  }
})
</script>

