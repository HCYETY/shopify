   }

    @media (min-width:0px) {
      .christmas-activities-page .fit_picture .placeholderTag {
          display: none;
      }
      .christmas-activities-page .fit_picture ._xmedium {
          display: block;
      }
    }
  
    @media (min-width:1921px) {
      .christmas-activities-page .fit_picture .placeholderTag {
          display: none;
      }
      .christmas-activities-page .fit_picture ._xlarge {
          display: block;
      }
    }
  
    @media (max-width:960px) {
      .christmas-activities-page .fit_picture .placeholderTag {
          display: none;
      }
      .christmas-activities-page .fit_picture ._xsmall {
          display: block;
      }
    }

