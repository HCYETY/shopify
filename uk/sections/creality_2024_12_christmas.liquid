<script>
   let ilist = {{ section.settings.introduce_block_list | split: "," | json }};

  
  gsap.registerPlugin(ScrollToPlugin);
  gsap.registerPlugin(ScrollTrigger)
  const delay = function (timer) {
    return new Promise((res, rej) => {
      setTimeout(() => {
        res()
      }, timer)
    })
  }
  
  const docWidth = document.documentElement.clientWidth;
  const docHeight = document.documentElement.clientHeight;
  const vw = docWidth / 100;
  const vh = docHeight / 100;
  let spaceBetween = 1.3 * vw;
  
  let spaceNum = docWidth > 960 ? 0.012 : 0.05
  
  if(docWidth > 960){
    new Swiper(".swiper1",{
      slidesPerView: 4,
      spaceBetween: 20,
      pagination: {
        el: ".swiper-pagination",
      },
      navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    })
  }else{
    new Swiper(".swiper1",{
      slidesPerView: 1,
      spaceBetween: 20,
      pagination: {
        el: ".swiper-pagination",
      }
    })
  }
  $('.faq-answer').first().slideDown();
  
  $('.faq-question').click(function () {
      var answer = $(this).next('.faq-answer');
  
      if (!answer.is(':visible')) {
          $('.faq-answer').not(answer).slideUp();
  
          answer.slideDown();
      }
  });
  // 倒计时
  $(function () {
    let cloneDiv = $(".gift-explain-item").clone();
    let pDiv = $(".gift-explain");
    pDiv.html("");
    ilist.map((v)=>{
      let n = cloneDiv.clone();
      $(n).find("p").text(v);
      pDiv.append(n);
    })
    
    var targetDate = new Date("{{ section.settings.countdown }}").getTime();
    console.log(targetDate)
    function updateCountdown() {
      var now = new Date().getTime();
      var timeLeft = targetDate - now;
      var days = Math.floor(timeLeft / (1000 * 60 * 60 * 24));
      var hours = Math.floor(
        (timeLeft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60)
      );
      var minutes = Math.floor((timeLeft % (1000 * 60 * 60)) / (1000 * 60));
      var seconds = Math.floor((timeLeft % (1000 * 60)) / 1000);
      $(".countdown .days").text(
        days 
      );
      $(".countdown .hours").text(
        hours
      );
      $(".countdown .minutes").text(
        minutes
      );
      $(".countdown .seconds").text(
        seconds
      );
      if (timeLeft < 0) {
        clearInterval(countdownInterval);
        $(".countdown .days").text(
          "0"
        );
        $(".countdown .hours").text(
          "0"
        );
        $(".countdown .minutes").text(
          "0"
        );
        $(".countdown .seconds").text(
          "0 "
        );
      }
    }
    var countdownInterval = setInterval(updateCountdown, 1000);
  });
  
  
  var fadeInListen = function () {
    var fadeIn = $('.main-contain').find('.fadein');
  
    fadeIn.each(function () {
      let element = this;
      let {
        scrub,
        reverse,
        duration,
        actions,
        fn
      } = $(this).data()
  
      gsap.set(element, {
        opacity: 0,
        y: 30
      });
      let toggleActions = "play none none none";
      let start = $(this).data('start') || 80;
      duration ? '' : duration = 0.8
      let tl = gsap.to(element, {
        opacity: 1,
        y: 0,
        duration
      });
      if (reverse) {
        toggleActions = "play none none reverse"
      }
  
      ScrollTrigger.create({
        animation: tl,
        trigger: element,
        toggleActions,
        start: `top ${start}%`, // 调整触发点，视情况而定
        onEnter: function () {
  
        },
        once: true // 只触发一次
      });
    });
  
    var delayFadeIn = $('.delay-fadein');
    delayFadeIn.each(function () {
      let element = this;
      let {
        parent,
        el,
        scrub,
        reverse,
        duration,
        actions,
        fn
      } = $(this).data()
      let parentEl = $(parent);
      parentEl.length ? '' : parentEl = $(element)
      let delayEl = [];
      if (el) {
        el.split(',').forEach((selector) => delayEl.push(...parentEl.find(
          selector)))
      } else {
        delayEl = parentEl.children();
      }
  
      let toggleActions = "play none none none";
      scrub ? scrub = true : scrub = false;
  
      duration ? '' : duration = 0.8
      console.log(duration)
      let start = $(this).data('start') || 80;
      let stagger = $(this).data('delay') || 0.3;
  
  
      gsap.set(delayEl, {
        opacity: 0,
        y: 30
      });
  
      let tl = gsap.timeline();
      tl.to(delayEl, {
        stagger,
        opacity: 1,
        y: 0,
        duration
      })
      if (fn) {
        console.log(fn)
        tl = k1c_tl[fn](tl)
      }
      if (reverse) {
        toggleActions = "play none none reverse"
        gsap.set(element, {
          opacity: 0,
          y: 30
        });
        let t2 = gsap.to(element, {
          opacity: 1,
          y: 0,
          duration
        })
        ScrollTrigger.create({
          animation: t2,
          toggleActions,
          trigger: element,
          scrub,
          start: `top ${start}%`, // 调整触发点，视情况而定
        });
      }
      actions ? toggleActions = actions : ''
  
  
      ScrollTrigger.create({
        animation: tl,
        toggleActions,
        trigger: element,
        scrub,
        start: `top ${start}%`, // 调整触发点，视情况而定
      });
    });
  }
  fadeInListen();
</script>

