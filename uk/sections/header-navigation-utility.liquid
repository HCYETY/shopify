                    let day = Math.floor(leftTime / (24 * 60 * 60));
                    let hour = Math.floor((leftTime / (60 * 60)) % 24);
                    let minute = Math.floor((leftTime / 60) % 60);
                    let second = Math.floor(leftTime % 60);
              
