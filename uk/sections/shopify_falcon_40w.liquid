<script>
    gsap.registerPlugin(ScrollToPlugin);
    gsap.registerPlugin(ScrollTrigger)
    const delay = function (timer) {
        return new Promise((res, rej) => {
            setTimeout(() => {
                res()
            }, timer)
        })
    }

    const docWidth = document.documentElement.clientWidth;
    const docHeight = document.documentElement.clientHeight;
    const vw = docWidth / 100;
    const vh = docHeight / 100;
    let spaceBetween = 1.3 * vw;

    let spaceNum = docWidth > 960 ? 0.012 : 0.05


    let tarGetinit = function () {
        $('[data-target]').click(function (e) {
            e.preventDefault();
            let target = $(this).data('target')
            gsap.to(window, {
                duration: 0.5,
                scrollTo: {
                    y: target
                }
            });
        })
        let pagePagination = $(".page-pagination")

        $(window).scroll(function () {
            if (docWidth > 960) {
                var scrollPos = $(window).scrollTop();
                let docHeight = document.documentElement.clientHeight

                $(".page-pagination li").each(function (key) {
                    var target = $(this).data("target");
                    var targetPos = $(target).offset().top;
                    var targetHeight = $(target).outerHeight();

                    // 
                    if (scrollPos >= (targetPos - docHeight * (1 - 0.3))) {
                        pagePagination.fadeIn();
                        $('.page-pagination li').removeClass('active')
                        $(this).addClass("active"); // "active"
                    } else if (key === 0) {
                        pagePagination.fadeOut()
                    }
                });
            }
        });
    }

    var fadeInListen = function () {
        var fadeIn = $('.main-contain').find('.fadein');

        fadeIn.each(function () {
            let element = this;
            let {
                scrub,
                reverse,
                duration,
                actions,
                fn
            } = $(this).data()

            gsap.set(element, {
                opacity: 0,
                y: 30
            });
            let toggleActions = "play none none none";
            let start = $(this).data('start') || 80;
            duration ? '' : duration = 0.8
            let tl = gsap.to(element, {
                opacity: 1,
                y: 0,
                duration
            });
            if (reverse) {
                toggleActions = "play none none reverse"
            }

            ScrollTrigger.create({
                animation: tl,
                trigger: element,
                toggleActions,
                start: `top ${start}%`, // 
                onEnter: function () {

                },
                once: true // 
            });
        });

        var delayFadeIn = $('.delay-fadein');
        delayFadeIn.each(function () {
            let element = this;
            let {
                parent,
                el,
                scrub,
                reverse,
                duration,
                actions,
                fn
            } = $(this).data()
            let parentEl = $(parent);
            parentEl.length ? '' : parentEl = $(element)
            let delayEl = [];
            if (el) {
                el.split(',').forEach((selector) => delayEl.push(...parentEl.find(
                    selector)))
            } else {
                delayEl = parentEl.children();
            }

            let toggleActions = "play none none none";
            scrub ? scrub = true : scrub = false;

            duration ? '' : duration = 0.8
            console.log(duration)
            let start = $(this).data('start') || 80;
            let stagger = $(this).data('delay') || 0.3;


            gsap.set(delayEl, {
                opacity: 0,
                y: 30
            });

            let tl = gsap.timeline();
            tl.to(delayEl, {
                stagger,
                opacity: 1,
                y: 0,
                duration
            })
            if (fn) {
                console.log(fn)
                tl = k1c_tl[fn](tl)
            }
            if (reverse) {
                toggleActions = "play none none reverse"
                gsap.set(element, {
                    opacity: 0,
                    y: 30
                });
                let t2 = gsap.to(element, {
                    opacity: 1,
                    y: 0,
                    duration
                })
                ScrollTrigger.create({
                    animation: t2,
                    toggleActions,
                    trigger: element,
                    scrub,
                    start: `top ${start}%`, // 
                });
            }
            actions ? toggleActions = actions : ''


            ScrollTrigger.create({
                animation: tl,
                toggleActions,
                trigger: element,
                scrub,
                start: `top ${start}%`, // 
                // marker: true
                // once: true // 
            });
        });

    }
    function throttle(func, delay) {
        let lastCall = 0;
        return function (...args) {
            const now = new Date().getTime();
            if (now - lastCall >= delay) {
                lastCall = now;
                return func(...args);
            }
        };
    }


    let videos = document.querySelectorAll('.auto-video');
    let tolerancePixel = 40;
    function checkVideosInView() {
        let scrollTop = $(window).scrollTop() + tolerancePixel;
        let scrollBottom = $(window).scrollTop() + $(window).height() - tolerancePixel;
        videos.forEach(function (video) {
            let yTopMedia = $(video).offset().top;
            let yBottomMedia = $(video).height() + yTopMedia;

            if (scrollTop < yBottomMedia && scrollBottom > yTopMedia) {
                if (video.readyState >= 3) { // HAVE_FUTURE_DATA or higher
                    video.play();
                } else {
                    video.addEventListener('canplaythrough', function onCanPlay() {
                        video.play();
                        video.removeEventListener('canplaythrough', onCanPlay);
                    });
                }
            } else {
                video.pause();
            }
        });
    }

    window.addEventListener('scroll', throttle(checkVideosInView, 150));
    window.addEventListener('resize', throttle(checkVideosInView, 150));

    checkVideosInView();

    //  
    fadeInListen();
    tarGetinit() //


    // <!-- lazy load -->
    const picture_observer = lozad('.fit_picture,.lazy-video', {
        rootMargin: '1500px 0px', // syntax similar to that of CSS Margin
        load: function (el) {
            // console.log(el)
            if (el.tagName === 'PICTURE' || el.tagName === 'IMG') {
                let image = el.tagName === 'IMG' ? el : el.querySelector('img');
                if (el.tagName === 'IMG') image = el;

                image.onload = function () {
                    this.classList.add('loaded')
                }
                el.querySelectorAll('source').forEach(source => {
                    let srcset = source.getAttribute('data-srcset');
                    srcset ? source.setAttribute('srcset', srcset) : ''
                });
            } else if (el.tagName === 'VIDEO') {
                el.onloadeddata = function () {
                    console.log('loaded...')
                    this.classList.add('loaded');
                };
                const poster = el.getAttribute('data-poster');
                if (poster) el.setAttribute('poster', poster);

                el.querySelectorAll('source').forEach(source => {
                    const src = source.getAttribute('data-src');
                    if (src) {
                        source.setAttribute('src', src);
                        el.load()
                    }
                });
            }
        }
    });
    picture_observer.observe();
</script>

