  $(function () {
    // 
    var targetDate = new Date("{{section.settings.countDown}}").getTime();

    // 
    function updateCountdown() {
      var now = new Date().getTime();
      var timeLeft = targetDate - now;

      // 
      var days = Math.floor(timeLeft / (1000 * 60 * 60 * 24));
      var hours = Math.floor(
        (timeLeft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60)
      );
      var minutes = Math.floor((timeLeft % (1000 * 60 * 60)) / (1000 * 60));
      var seconds = Math.floor((timeLeft % (1000 * 60)) / 1000);

      // 
      $(".halloween-activity-section2-box-p .days").html(
        days + " <span>Days</span>"
      );
      $(".halloween-activity-section2-box-p .hours").html(
        hours + " <span>Hours</span>"
      );
      $(".halloween-activity-section2-box-p .minutes").html(
        minutes + " <span>Minutes</span>"
      );
      $(".halloween-activity-section2-box-p .seconds").html(
        seconds + " <span>Seconds</span>"
      );

      // "EXPIRED"
      if (timeLeft < 0) {
        clearInterval(countdownInterval);
        $(".halloween-activity-section2-box-p .days").html(
          "0 <span>Days</span>"
        );
        $(".halloween-activity-section2-box-p .hours").html(
