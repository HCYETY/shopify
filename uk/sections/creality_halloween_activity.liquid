  $(function () {
    const scrollableTable = document.querySelector(
      ".halloween-activity-section10-box-list-table"
    );

    let isDragging = false; // 是否正在拖动
    let startX; // 鼠标起始位置
    let scrollLeft; // 初始滚动位置

    scrollableTable.addEventListener("mousedown", (e) => {
      isDragging = true;
      startX = e.pageX - scrollableTable.offsetLeft; // 获取鼠标按下时的 X 坐标
      scrollLeft = scrollableTable.scrollLeft; // 记录当前的滚动位置
      scrollableTable.style.cursor = "grabbing"; // 更改鼠标样式为抓取状态
    });

    scrollableTable.addEventListener("mouseleave", () => {
      isDragging = false; // 离开区域时停止拖动
      scrollableTable.style.cursor = "grab"; // 恢复鼠标样式
    });

    scrollableTable.addEventListener("mouseup", () => {
      isDragging = false; // 松开鼠标时停止拖动
      scrollableTable.style.cursor = "grab"; // 恢复鼠标样式
    });

    scrollableTable.addEventListener("mousemove", (e) => {
      if (!isDragging) return; // 如果未按下鼠标，则返回
      e.preventDefault(); // 阻止默认行为
      const x = e.pageX - scrollableTable.offsetLeft; // 获取当前鼠标位置
      const walk = (x - startX) * 2; // 计算拖动的距离（倍数可调整）
      scrollableTable.scrollLeft = scrollLeft - walk; // 设置水平滚动
    });
  });
