      width: totalWidth + "px",
      animation: `slide ${(totalWidth / itemWidth) * 5}s linear infinite`, // 
    });

    // 
    var keyframes = `
                @keyframes slide {
                    0% {
                        transform: translateX(0);
                    }
                    100% {
                        transform: translateX(-${totalWidth}px); /*  */
                    }
                }
            `;
    var style = document.createElement("style");
    style.type = "text/css";
    style.appendChild(document.createTextNode(keyframes));
    document.head.appendChild(style);

    // 
    $(".halloween-activity-section9-box-list-swiper")
      .on("mouseenter", function () {
        $swiperDiv.css("animation-play-state", "paused");
      })
      .on("mouseleave", function () {
        $swiperDiv.css("animation-play-state", "running");
      });
  });
  //section10
  $(function () {
    const scrollableTable = document.querySelector(
      ".halloween-activity-section10-box-list-table"
    );

    let isDragging = false; // 
    let startX; // 
    let scrollLeft; // 

    scrollableTable.addEventListener("mousedown", (e) => {
      isDragging = true;
      startX = e.pageX - scrollableTable.offsetLeft; //  X 
      scrollLeft = scrollableTable.scrollLeft; // 
      scrollableTable.style.cursor = "grabbing"; // 
    });

    scrollableTable.addEventListener("mouseleave", () => {
      isDragging = false; // 
      scrollableTable.style.cursor = "grab"; // 
    });

    scrollableTable.addEventListener("mouseup", () => {
      isDragging = false; // 
      scrollableTable.style.cursor = "grab"; // 
    });

    scrollableTable.addEventListener("mousemove", (e) => {
      if (!isDragging) return; // 
      e.preventDefault(); // 
      const x = e.pageX - scrollableTable.offsetLeft; // 
      const walk = (x - startX) * 2; // 
      scrollableTable.scrollLeft = scrollLeft - walk; // 
    });
  });
