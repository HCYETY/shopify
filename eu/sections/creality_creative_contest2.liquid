      if (res.code === 200) {
        // 假设数据结构是一个数组
        const videoItems = res.rows;
        const imgUrls = [
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1144.png",
          "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/1143.png",
