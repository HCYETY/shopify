  });

  // 获取弹窗元素
  var $modal = $('#lotteryModal');
  var $closeBtn = $modal.find('.close');
  var $form = $('#lotteryForm');
  var $giftModal = $('#giftModal');
  var $giftCloseBtn = $giftModal.find('.close');

  // 显示弹窗的函数
  function showModal() {
    $modal.fadeIn();
  }

  // 隐藏弹窗的函数
  function closeModal() {
    $modal.fadeOut();
  }

  // 点击关闭按钮时隐藏弹窗
  $closeBtn.on('click', closeModal);

  // 点击窗口外部时隐藏弹窗
  $(window).on('click', function(event) {
    if ($(event.target).is($modal)) {
      closeModal();
    }
  });

  $(".prize-draw").click(function () {
    showModal();
  });

  // 显示中奖弹窗的函数
  function showGiftModal(res) {
    // setTimeout(function() {
    //   $('#giftModal .gift-img').removeClass('shake').addClass('open');
    // }, 1000);

    if (res.targetIndex != 2) {
      $('#giftModal .title').show();
      $('#giftModal .gift-name').show();
      $('#giftModal .tips').show();
      $('#giftModal .gift-name').text(res.prizeName);
    } else {
      $('#giftModal .title').hide();
      $('#giftModal .gift-name').hide();
      $('#giftModal .tips').hide();
    }
    $("#giftModal .gift-img").attr("src", res.img);
    $giftModal.fadeIn();
  }

  // showGiftModal({targetIndex: 1, img: "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/20241223-154140.webp?v=1734939818"});
  // showGiftModal({targetIndex: 2, img: "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/796896641e4d484d20c5fcfa5f429676.png?v=1734939779"});
  
  // 隐藏中奖弹窗的函数
  function closeGiftModal() {
    $giftModal.fadeOut();
  }

  $giftCloseBtn.on('click', closeGiftModal);

  // 抽奖提交
  $form.on('submit', function(event) {
    event.preventDefault();
    var email = $('#lotteryEmail').val();

    $.ajax({
      url: 'https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/v2/participation',
      method: 'POST',
      contentType: 'application/json',
      data: JSON.stringify({ email: email }),
      success: function(response) {
        if (response.code === 200) {
          closeModal();
          showGiftModal(response.result); 
        } else if (response.code === 3012) {
          showToast("warning", "Please check, you don't meet the lottery conditions currently.");
        } else {
          showToast("warning", "An unexpected error occurred. Please try again later.");
        }
      },
      error: function() {
        showToast("error", "Network error, please try again later");
      }
    });
  });

  // Form submission with AJAX
  $('#infoForm').on('submit', function (event) {
    event.preventDefault(); // Prevent the default form submission

    // Form data
    var formData = $(this).serializeArray();
    var jsonData = {};

    $.each(formData, function (i, field) {
      jsonData[field.name] = field.value; // Construct JSON object
    });

    if (jsonData) {
      jsonData.hasFalcon = Boolean(jsonData.hasFalcon)
    }

    // AJAX request
    $.ajax({
      type: 'POST',
      url: 'https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/v2/form', // Replace with your server endpoint
      data: JSON.stringify(jsonData),
      dataType: 'json',
      contentType: 'application/json',
      success: function (response) {
        alert('Form submitted successfully!');
        $('#formModal').hide();
        $('#infoForm')[0].reset();
        $.toast({
          type: 'success',
          duration: 3000,
          text: 'Submitted successfully!',
          icon: 'success',
          position: 'mid-center',  // 中间位置
          loader: true,        // Change it to false to disable loader
          loaderBg: '#9EC600'  // To change the background
        });
