      error: function (jqXHR, textStatus, errorThrown) {
        // console.error('Error:', textStatus, errorThrown); // Improved error logging
        // alert('An error occurred. Please try again.');
      }
    });
  });

  $.ajax({
    url: 'https://bsplugins.creality.com/openapi/dtc/crealityFalcon/videoChallenge/v2/list?pageNum=1&pageSize=6', // 替换为你的数据源
