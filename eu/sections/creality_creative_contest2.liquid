                  </div>
                  <div class="nums HarmonyOS_Regular">${video.videoViews}</div>
                </div>
                <div class="item">
                  <div class="img-wrap">
                    <img class="icon" src="https://cdn.shopify.com/s/files/1/0799/8714/7049/files/016aea119159532004f23b2411acf908.png?v=1725444900" alt="">
                  </div>
                  <div class="nums HarmonyOS_Regular">${video.videoComments}</div>
                </div>
              </div>
            </div>
          `;

          $('.section3_2 .swiper .swiper-wrapper').append(videoHtml);
        });
  
        // 更新页面信息
        updatePageInfo();
      } else {
        console.log('请求失败:', res.message);
      }
    },
    error: function () {
      console.log('无法获取数据');
    }
  });
  
  // 从 YouTube 视频 URL 中提取视频 ID
  function extractVideoId(url) {
    const match = url.match(/youtube\.com\/watch\?v=([^\s&]+)/) || url.match(/youtu\.be\/([^\s&]+)/);
    return match ? match[1] : '';
  }

  function showToast(type, message) {
    const options = {
        duration: 3000,
        text: message,
        position: 'mid-center',
        loader: true,
        loaderBg: type === 'error' ? 'red' : type === 'warning' ? 'yellow' : 'green', // 根据类型设置背景颜色
    };

    switch (type) {
        case 'error':
            options.type = 'error';
            options.icon = 'error';
            break;
        case 'warning':
            options.type = 'warning';
            options.icon = 'warning';
            break;
        case 'success':
            options.type = 'success';
            options.icon = 'success';
            break;
        default:
            options.type = 'info';
            options.icon = 'info';
            break;
    }

    $.toast(options);
  }
})
</script>

