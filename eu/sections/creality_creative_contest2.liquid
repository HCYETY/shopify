  // 点击蒙层关闭弹窗
  $('#openModal').click(function () {
    $('#formModal').fadeIn();
  });

  $(".updata-btn").click(function () {
    $('#formModal').fadeIn();
  });

  // Close modal
  $('#formModal .close').click(function () {
    $('#formModal').fadeOut();
  });

  // Close modal if clicked outside the modal
  $(window).click(function (event) {
    if ($(event.target).is('#formModal')) {
      $('#formModal').hide();
