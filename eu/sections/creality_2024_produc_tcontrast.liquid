  $(function () {
  let isClicking = false;
  $(".contrast-nav-box p").on("click", function () {
    isClicking = true;
    $(".contrast-nav-box p").removeClass("active");
    $(this).addClass("active");
    $(".strip p").css({
      top: `${$(this).attr("value")}%`,
    });
    const targetId = $(this).attr("data");
    slide(targetId);
  });
  
  function slide(targetId) {
    $("html, body").animate(
      {
        scrollTop: $(`#${targetId}`).offset().top - 150,
      },
      1000,
      function () {
        isClicking = false; // 重置标志
      }
    );
    $(`#${targetId}`).parent().siblings().removeClass("active");
    $(`#${targetId}`).siblings().children("svg").css({
      transform: "rotate(180deg)",
    });
  }
  $(window).on("scroll", function () {
    if (isClicking) return;
    const scrollPosition = $(window).scrollTop() + 150; // Adjust based on the offset used in slide
    $(".contrast-nav-box p").each(function (item) {
      const targetId = $(this).attr("data");
      const targetOffset = $(`#${targetId}`).offset().top;
      const targetHeight = $(`#${targetId}`)
        .parent()
        .parent()
        .parent()
        .outerHeight();
  
      if (
        scrollPosition >= targetOffset &&
        scrollPosition < targetOffset + targetHeight
      ) {
        $(".contrast-nav-box p").removeClass("active");
        $(this).addClass("active");
        $(".strip p").css({
          top: `${$(this).attr("value")}%`,
        });
      }
    });
  });
  });
