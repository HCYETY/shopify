        top:  "50%",
      });
      }

      }
 

    } else {
      $("#MainContent").css({
        overflow: "hidden",
      });
    //  $(".scrolled-past-header").show();
      $(".contrast-nav").hide();
    }
    let h=80
    let ht=0
    if ($("#qab_background").length) {
      ht=$("#qab_background").height()
      h=Number(80)+Number($("#qab_background").height())
    }
    
    if (navTop <= h) {
      $(".contrast-nav").addClass("navactive");
      $(".contrast-header").css({
        boxShadow: "none",
      });
    } else {
      $(".contrast-nav").removeClass("navactive");
      $(".contrast-header").css({
        boxShadow: "0px 5px 5px #f1eeee",
      });
    }
    if (headerTop > ht) {
        $(".contrast-header").css({
        boxShadow: "none",
      });
    }
 
  });

});


