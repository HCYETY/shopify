    "faqs_ENiAPH": {
      "type": "faqs",
      "blocks": {
        "question_item_7dTFfU": {
          "type": "question_item",
          "settings": {
            "question_text": "Q1: How Can I get all the free gifts during this event?",
            "unswer_text": "If your order meets the requirements of our promotion, the free gifts will be automatically added to your shopping cart until you reach the checkout page. If you don't see it, please contact our customer service for assistance. Please email to: eu.support@crealityfalcon.com.",
            "is_open": false
          }
        },
        "question_item_FidjkV": {
          "type": "question_item",
          "settings": {
            "question_text": "Q2：What payment methods do you accept?",
            "unswer_text": "We accept a variety of payment methods including credit cards, shop pay, PayPal, Klarna and more. You can choose the payment method that works best for you at checkout.",
            "is_open": false
          }
        },
        "question_item_rEW8JR": {
          "type": "question_item",
          "settings": {
            "question_text": "Q3：How long does shipment normally take?",
            "unswer_text": "We typically ship packages within 48 hours. Delivery time is estimated to take 5-10 working days from the German warehouse.",
            "is_open": false
          }
        },
        "question_item_UhGpQN": {
          "type": "question_item",
          "settings": {
            "question_text": "Q4：Can I engrave on reflective/transparent materials such as ceramic/glass?",
            "unswer_text": "Yes, but you need to coat the surface of the material with anti-reflective material before engraving to ensure the engraving effect and prevent the reflected light from damaging the laser module.",
            "is_open": false
          }
        }
      },
