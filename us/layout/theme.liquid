<!doctype html>
{%- liquid
  if settings.list_iso_rtl != blank
    assign locale_iso_code = request.locale.iso_code
    assign arr_list_rtl = settings.list_iso_rtl | remove: ' ' | split: ','
    if arr_list_rtl contains locale_iso_code
      assign enable_iso_rtl = true
    else
      assign enable_iso_rtl = false
    endif
  endif
-%}
<html
  class="no-js"
  lang="{{ request.locale.iso_code }}"
  {% if settings.enable_layout_rtl and enable_iso_rtl %}
    dir="rtl"
  {% endif %}
>
  <script type="text/javascript">
    (function (c, l, a, r, i, t, y) {
      c[a] =
        c[a] ||
        function () {
          (c[a].q = c[a].q || []).push(arguments);
        };
      t = l.createElement(r);
      t.async = 1;
      t.src = 'https://www.clarity.ms/tag/' + i;
      y = l.getElementsByTagName(r)[0];
      y.parentNode.insertBefore(t, y);
    })(window, document, 'clarity', 'script', 'qtijqd5rgt');
  </script>

  <head>
    <!-- SEO START - Optimized hreflang links -->
    {%- liquid
      assign hreflang_urls = ''
      case request.path
        when '/products/falcon-a1-10w-enclosed-laser-engraver-and-cutter'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/falcon-a1-10w-enclosed-laser-cutter-and-engraver,en-gb&https://uk.crealityfalcon.com/products/creality-falcon-a1-laser-engraver-cutter,en&https://www.crealityfalcon.com/products/falcon-a1-10w-enclosed-laser-engraver-and-cutter,x-default&https://www.crealityfalcon.com/products/falcon-a1-10w-enclosed-laser-engraver-and-cutter'
        when '/products/low-wattage-laser-engraver-cutter-cr-falcon-10w-machine'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/low-wattage-engraver-cr-laser-falcon-10w,en-gb&https://uk.crealityfalcon.com/products/cr-laser-falcon-engraver-10w-low-wattage,en&https://www.crealityfalcon.com/products/low-wattage-laser-engraver-cutter-cr-falcon-10w-machine,x-default&https://www.crealityfalcon.com/products/low-wattage-laser-engraver-cutter-cr-falcon-10w-machine'
        when '/products/low-wattage-laser-engraver-cutter-cr-falcon-5w'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/low-wattage-engraver-cr-laser-falcon-5w-for-beginners,en-gb&https://uk.crealityfalcon.com/products/cr-laser-falcon-5w-laser-engraver,en&https://www.crealityfalcon.com/products/low-wattage-laser-engraver-cutter-cr-falcon-5w,x-default&https://www.crealityfalcon.com/products/low-wattage-laser-engraver-cutter-cr-falcon-5w'
        when '/products/falcon2-22w-laser-engraver-cutter'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/falcon2-22w-laser-engraver-and-cutter,en-gb&https://uk.crealityfalcon.com/products/falcon2-22w-laser-engraver-cutter,en&https://www.crealityfalcon.com/products/falcon2-22w-laser-engraver-cutter,x-default&https://www.crealityfalcon.com/products/falcon2-22w-laser-engraver-cutter'
        when '/products/falcon-2-pro-40w-enclosed-laser-engraver-and-cutter'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/falcon2-pro-40w-and-22w-enclosed-laser-cutter-engraver,en-gb&https://uk.crealityfalcon.com/products/falcon2-pro-enclosed-laser-engraver-cutter,en&https://www.crealityfalcon.com/products/falcon-2-pro-40w-enclosed-laser-engraver-and-cutter,x-default&https://www.crealityfalcon.com/products/falcon-2-pro-40w-enclosed-laser-engraver-and-cutter'
        when '/products/laser-engraver-cutter-falcon2-40w-engraving-machine'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/products/falcon2-40w-laser-engraver-cutter,en-gb&https://uk.crealityfalcon.com/products/falcon2-40w-laser-engraver-and-cutter,en&https://www.crealityfalcon.com/products/laser-engraver-cutter-falcon2-40w-engraving-machine,x-default&https://www.crealityfalcon.com/products/laser-engraver-cutter-falcon2-40w-engraving-machine'
        when '/collections/engravers'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/collections/laser-engraver-and-cutter,en-gb&https://uk.crealityfalcon.com/collections/engraver,en&https://www.crealityfalcon.com/collections/engravers,x-default&https://www.crealityfalcon.com/collections/engravers'
        when '/collections/accessories-for-falcon-laser-engraver'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/collections/engraving-machine-accessories,en-gb&https://uk.crealityfalcon.com/collections/accessories,en&https://www.crealityfalcon.com/collections/accessories-for-falcon-laser-engraver,x-default&https://www.crealityfalcon.com/collections/accessories-for-falcon-laser-engraver'
        when '/pages/materials-sale-for-laser-engraver'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/collections/engraving-machine-craft-materials,en-gb&https://uk.crealityfalcon.com/pages/materials-sale-for-laser-engraver,en&https://www.crealityfalcon.com/pages/materials-sale-for-laser-engraver,x-default&https://www.crealityfalcon.com/pages/materials-sale-for-laser-engraver'
        when '/'
          assign hreflang_urls = 'en-au&https://au.crealityfalcon.com/,en-gb&https://uk.crealityfalcon.com/,en&https://www.crealityfalcon.com/,x-default&https://www.crealityfalcon.com/'
      endcase
    -%}
    {%- if hreflang_urls != blank -%}
      {%- assign url_pairs = hreflang_urls | split: ',' -%}
      {%- for pair in url_pairs -%}
        {%- assign lang_url = pair | split: '&' -%}
        <link rel="alternate" hreflang="{{ lang_url[0] }}" href="{{ lang_url[1] }}">
      {%- endfor -%}
    {%- endif -%}
    <!-- SEO END -->

    <!-- EASYGIFT SCRIPT -->
    <script
      defer
      type="text/javascript"
      src="https://cdn.506.io/eg/script.js?shop={{ shop.permanent_domain }}{% if customer %}&id={{ customer.id | escape_once }}&order_count={{ customer.orders_count | escape_once }}&total_spent={{ customer.total_spent | escape_once }}&tags={{ customer.tags | join: "," | url_encode }}{% endif %}"
    ></script>
    <!-- END EASYGIFT SCRIPT -->
    <!-- BEGIN MerchantWidget Code -->
    <script
      id="merchantWidgetScript"
      src="https://www.gstatic.com/shopping/merchant/merchantwidget.js"
      defer
    ></script>
    <script type="text/javascript">
      merchantWidgetScript.addEventListener('load', function () {
        merchantwidget.start({
          position: 'RIGHT_BOTTOM',
          sideMargin: 21,
          bottomMargin: 95,
          mobileSideMargin: 11,
          mobileBottomMargin: 60,
        });
      });
    </script>
    <!-- END MerchantWidget Code -->
    <script async src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/4.1.1/crypto-js.min.js"></script>

    <!-- Google Tag Manager -->
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ 'gtm.start': new Date().getTime(), event: 'gtm.js' });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != 'dataLayer' ? '&l=' + l : '';
        j.async = true;
        j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, 'script', 'dataLayer', 'GTM-WNQQSMDJ');
    </script>
    <!-- End Google Tag Manager -->

    {%- render 'ecom_header' -%}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{% render 'canonical-url-mapping' %}">
    {%- if settings.favicon != blank -%}
      <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
    {%- endif -%}
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
    </title>

    {%- if page_description -%}
      <meta name="description" content="{{ page_description | escape }}">
    {%- endif -%}
    {% render 'meta-tags' %}
    {{ content_for_header }}
    {% render 'variable' %}
    {% render 'global-style' %}
    {% render 'global-script' %}

    {% render 'zbanx-script-head' %}

    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
    </script>

    {%- liquid
      if settings.layout_body == 'full'
        assign layout_body = 'body-full-width'
      elsif settings.layout_body == 'boxed'
        assign layout_body = 'body-boxed'
      elsif settings.layout_body == 'custom_width'
        assign layout_body = 'body-custom-width'
      endif

      if settings.style_dots == '02'
        assign style_dots = 'style-dots-2'
      endif

      if settings.style_arrows == '02'
        assign style_arrows = 'style-arrows-2'
      elsif settings.style_arrows == '05'
        assign style_arrows = 'style-arrows-5'
      endif

      if settings.product_card_layout == '02'
        assign product_card_layout = 'product-card-layout-02'
      elsif settings.product_card_layout == '03'
        assign product_card_layout = 'product-card-layout-03'
      elsif settings.product_card_layout == '04'
        assign product_card_layout = 'product-card-layout-04'
      elsif settings.product_card_layout == '05'
        assign product_card_layout = 'product-card-layout-05'
      elsif settings.product_card_layout == '06'
        assign product_card_layout = 'product-card-layout-06'
      elsif settings.product_card_layout == '07'
        assign product_card_layout = 'product-card-layout-07'
      else
        assign product_card_layout = 'product-card-layout-01'
      endif

      if settings.quick_shop_type == '2'
        assign quick_shop_type = 'quick_shop_option_2'
      elsif settings.quick_shop_type == '3'
        assign quick_shop_type = 'quick_shop_option_3'
      endif

      if settings.style_text_size_varriant == '02'
        assign style_text_size_varriant = 'size_varriant_style_2'
      endif
    -%}
    {% render 'gp-head' %}

    <!-- SAP Emarsys -->
    <script type="text/javascript">
      var ScarabQueue = ScarabQueue || [];
      (function (id) {
        if (document.getElementById(id)) return;
        var js = document.createElement('script');
        js.id = id;
        js.src = '//cdn.scarabresearch.com/js/1EA4F52A13F57709/scarab-v2.js';
        var fs = document.getElementsByTagName('script')[0];
        fs.parentNode.insertBefore(js, fs);
      })('scarab-js-api');
    </script>
    <!-- End SAP Emarsys -->

    {% include 'pagefly-app-header' %}

    {%- render 'web-emarsys-sdk' -%}
  </head>
  <body class="template-{{ request.page_type }} {{layout_body}} {{style_dots}} {{style_text_size_varriant}} {{style_arrows}} {{product_card_layout}} {% if settings.enable_button_disable_arrows %} enable_button_disable_arrows{% endif %}{% if settings.display_text_color_title %} enable_text_color_title{% if settings.style_text_color_varriant == '02' %} style_2_text_color_varriant{% endif %}{% endif %}{% if settings.hide_compare_homepage %} hide_compare_homepage{% endif %} {{ quick_shop_type }}{% if settings.show_image_loading %} show_image_loading{% endif %}{% if settings.show_effect_close %} show_effect_close{% endif %}{% if settings.enable_layout_rtl and enable_iso_rtl %} layout_rtl{% endif %}{% if request.design_mode %} shopify-design-mode{% endif %}{% if settings.animations_hover_elements != 'none' %} animate--hover-{{ settings.animations_hover_elements }}{% endif %}{% if settings.enable_following_cursor and settings.following_cursor_style == 'default' %} cursor-fixed__show{% endif %}{% unless settings.enable_currencies %} disable_currencies{% endunless %}">
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-WNQQSMDJ"
        height="0"
        width="0"
        style="display:none;visibility:hidden"
      ></iframe
    ></noscript>
    <!-- End Google Tag Manager (noscript) -->

    <div class="body-content-wrapper">
      <a class="skip-to-content-link button visually-hidden" href="#MainContent">
        {{- 'accessibility.skip_to_text' | t -}}
      </a>
      {% render 'preload-screen' %}
      {% sections 'header-group' %}
      {% render 'wrapper-header' %}

      {%- if settings.layout_body == 'boxed' -%}
        <div class="wrapper-hasbanner-adv">
      {%- endif -%}
      {% render 'halo-banner-adv-left' %}
      {% render 'halo-banner-adv-right' %}
      <main id="MainContent" class="wrapper-body content-for-layout focus-none" role="main" tabindex="-1">
        {{ content_for_layout }}
      </main>
      {%- if settings.layout_body == 'boxed' -%}
        </div>
      {%- endif -%}

      <div class="wrapper-footer">
        <!-- ECOM-FOOTER -->
        {%- comment -%}eyUgc2VjdGlvbnMgJ2Zvb3Rlci1ncm91cCcgJX0={%- endcomment -%}
        {%- sections 'ecom-footer_footer' -%}
        <!-- /ECOM-FOOTER -->
      </div>

      {% section 'halo-toolbar-mobile' %}

      <ul hidden>
        <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      </ul>

      {% render 'global-script-2' %}
    </div>
    {% render 'halo-popup' %}

    {%- if settings.enable_view_documentation -%}
      {% render 'theme-editor-notification' %}
    {%- endif -%}

    {%- if settings.use_custom_js -%}
      {{ 'custom.js' | asset_url | script_tag }}
    {%- endif -%}
    <!-- chat-app -->
    {% render 'chat-app' %}

    <div class="background-overlay">
      {%- if settings.show_effect_close -%}
        <div class="background-cursor-wrapper">
          <div class="background-cursor-wrapper_outer">
            <div class="background-cursor-wrapper_inner">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48" width="20" height="20">
                <g class="nc-icon-wrapper" fill="#ffffff" transform="translate(0.5, 0.5)"><line fill="none" stroke-width="6" x1="46" y1="2" x2="2" y2="46"/> <line fill="none" stroke-width="6" x1="46" y1="46" x2="2" y2="2"/></g>
              </svg>
            </div>
          </div>
        </div>
      {%- endif -%}
    </div>

    {% render 'halo-sidebar' %}

    {%- if settings.enable_following_cursor -%}
      {% render 'cursor-fixed', style_cursor: settings.following_cursor_style %}
    {%- endif -%}

    <!-- {%  render 'web-push' %} -->
    {%- render 'ecom_footer' -%}
    <!-- 准备埋点获取的数据 -->
    <script>
        window.dataLayer = window.dataLayer || [];
          var productCollections = [];
          var collection ={{collection  | json}};
        {% for collection in product.collections %}
          productCollections.push("{{ collection.title }}");
        {% endfor %}
          var cartData = {{ cart | json }};
           var productData ={{ product | json}}
         if (productData && productData.variants) {
          var variants = productData.variants;
      } else {
          var variants = "";
      }
         var MD5useEmail=""
        if('{{ customer.email }}'){
           MD5useEmail=CryptoJS.MD5('{{ customer.email }}').toString()
        }
          var taxonomy_category={{product.category.ancestors | json}}
          if(taxonomy_category&&taxonomy_category.length >0){
            taxonomy_category=taxonomy_category.reverse().join(">")
          }else{
            taxonomy_category={{product.category | json}}
          }
        dataLayer.push({
          'event': 'userLoggedIn',
          'userEmail': MD5useEmail,
          'userid':'{{ customer.id }}',
          'variantID': '{{ product.id }}',
          'cartData':cartData.items,
          'country': '{{ customer.default_address.country }}',
          'productData':variants,
          'productId':'{{product.type}}',
          'productCollections':productCollections,
          "collection": taxonomy_category
        });
    </script>
  </body>
</html>
