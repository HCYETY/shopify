                    navigation: {
                        nextEl: '.swiper-button-next',
                        prevEl: '.swiper-button-prev',
                    },
                    on: {
                        slideChange: function(){
                        let selectEl = _this.parent.find('.cl-swiper--nav-item')[this.activeIndex % _this.ListLength];
                        let descEl = _this.parent.find('.cl-swiper-nav-list-desc .cl-swiper--nav-item')[this.activeIndex % _this.ListLength];
                        $(selectEl).addClass('cl-swiper-active').siblings().removeClass('cl-swiper-active');
                        descEl && $(descEl).addClass('cl-swiper-desc-active').siblings().removeClass('cl-swiper-desc-active');
                        _this.active = _this.parent.find('.cl-swiper-active');
                        _this.drag.css({
                            width: _this.active.innerWidth()+'px',
                            transform: `translateX(${_this.active.position().left - _this.mgLeft}px)`
                        });
                        }
                    },
                    ...options
                    });
                    
                    this.parent = $(selector).parent();
                    this.active = this.parent.find('.cl-swiper-active');
                    this.activeSwiperIndex = 0;
                    this.mgLeft = this.active.position()?.left;
                    this.drag = this.parent.find('.cl-swiper-nav-scrollbar-drag');
                    this.ListLength = this.parent.find('.cl-swiper--nav-item').length;
                    setTimeout(() => {this.active.click();})
                    this.init();
                }

                init(){
                    this.drag.css({width: this.active.innerWidth()+'px'});
                    this.parent.find('.cl-swiper-nav-list').on('click', (e) => {
                    // debugger
                    if(e.target.classList.contains('cl-swiper--nav-item')){ 
                        this.active = $(e.target);
                        this.activeSwiperIndex = this.active.data('index');
                        this.active.addClass('cl-swiper-active').siblings().removeClass('cl-swiper-active');
                        this.drag.css({
                        width: this.active.innerWidth()+'px',
                        transform: `translateX(${this.active.position().left - this.mgLeft}px)`
                        });
                        this.swiper.slideTo(this.activeSwiperIndex);
                    }
                    })
                }

                resize(){
                    this.mgLeft = $('.cl-swiper-nav-list').position().left;
                    this.drag.css({
                    width: this.active.innerWidth()+'px',
                    transform: `translateX(${this.active.position().left - this.mgLeft}px)`
                    });
                }
                }

                var fadeInListen = function () {
                var fadeIn = $('.main-contain').find('.fadein');

                fadeIn.each(function () {
                    let element = this;
                    let {
                    scrub,
                    reverse,
                    duration,
                    actions,
                    fn
                    } = $(this).data()

                    gsap.set(element, {
                    opacity: 0,
                    y: 30
                    });
                    let toggleActions = "play none none none";
                    let start = $(this).data('start') || 80;
                    duration ? '' : duration = 0.8
                    let tl = gsap.to(element, {
                    opacity: 1,
                    y: 0,
                    duration
                    });
                    if (reverse) {
                    toggleActions = "play none none reverse"
                    }

                    ScrollTrigger.create({
                    animation: tl,
                    trigger: element,
                    toggleActions,
                    start: `top ${start}%`, // 调整触发点，视情况而定
                    onEnter: function () {

                    },
                    once: true // 只触发一次
                    });
                });

                var delayFadeIn = $('.delay-fadein');
                delayFadeIn.each(function () {
                    let element = this;
                    let {
                    parent,
                    el,
                    scrub,
                    reverse,
                    duration,
                    actions,
                    fn
                    } = $(this).data()
                    let parentEl = $(parent);
                    parentEl.length ? '' : parentEl = $(element)
                    let delayEl = [];
                    if (el) {
                    el.split(',').forEach((selector) => delayEl.push(...parentEl.find(
                        selector)))
                    } else {
                    delayEl = parentEl.children();
                    }

                    let toggleActions = "play none none none";
                    scrub ? scrub = true : scrub = false;

                    duration ? '' : duration = 0.8
                    console.log(duration)
                    let start = $(this).data('start') || 80;
                    let stagger = $(this).data('delay') || 0.3;


                    gsap.set(delayEl, {
                    opacity: 0,
                    y: 30
                    });

                    let tl = gsap.timeline();
                    tl.to(delayEl, {
                    stagger,
                    opacity: 1,
                    y: 0,
                    duration
                    })
                    if (fn) {
                    console.log(fn)
                    tl = k1c_tl[fn](tl)
                    }
                    if (reverse) {
                    toggleActions = "play none none reverse"
                    gsap.set(element, {
                        opacity: 0,
                        y: 30
                    });
                    let t2 = gsap.to(element, {
                        opacity: 1,
                        y: 0,
                        duration
                    })
                    ScrollTrigger.create({
                        animation: t2,
                        toggleActions,
                        trigger: element,
                        scrub,
                        start: `top ${start}%`, // 调整触发点，视情况而定
                    });
                    }
                    actions ? toggleActions = actions : ''


                    ScrollTrigger.create({
                    animation: tl,
                    toggleActions,
                    trigger: element,
                    scrub,
                    start: `top ${start}%`, // 调整触发点，视情况而定
                    // marker: true
                    // once: true // 只触发一次
                    });
                });

                }

