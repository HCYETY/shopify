                spaceBetween: docWidth > 960 ? 30 : 18, 
                scrollbar: {
                    el: ".swiper-scrollbar",
                    draggable: true,
                },
                });

                var materialSwiper = new Swiper(".swiper-material", {
                slidesPerView: docWidth > 960 ? 4.5 : 3,
                spaceBetween: docWidth > 960 ? 10 : 0,
                ...(docWidth < 960 ? {allowTouchMove: false }: {})
                });

                new Swiper(".swiper1", {
                slidesPerView: 1,
                spaceBetween: 10, 
                });
                // let ns1 = new NavSwiper(".swiper1",{slidesPerView: 1});
                let ns2 = new NavSwiper(".swiper2");
                let ns3 = new NavSwiper(".swiper3");
                let ns4 = new NavSwiper(".swiper4",{slidesPerView: 1, ...(docWidth < 960 ? {allowTouchMove: false }: {})});

                $('.image-hover').mouseenter((e)=>{
                $(e.currentTarget).addClass('act').siblings().removeClass('act');
                })

                $('.software-item').click((e)=>{
                $(e.currentTarget).addClass('software-active').siblings().removeClass('software-active');
                let targetPosition = $(e.currentTarget).index();
                $('.software-video').eq(targetPosition).addClass('software-video-active').siblings().removeClass('software-video-active');
                })

                const videos = $(".auto-video");

                const observerOptions = {
                root: null, // 使用视口作为根
                rootMargin: "0px", // 无额外边距
                threshold: 0.5, // 当 50% 的元素进入视口时触发
                };

                const observer = new IntersectionObserver((entries) => {
                entries.forEach((entry) => {
                    const video = entry.target;

                    if (entry.isIntersecting) {
                    video.play();

                    if (video.hasAttribute("play-one")) {
                        video.addEventListener(
                        "ended",
                        () => {
                            observer.unobserve(video);
                        },
                        { once: true } // 确保事件只触发一次
                        );
                    }
                    } else {
                    video.pause();
                    video.currentTime = 0; // 重置视频到开头
                    }
                });
                }, observerOptions);

                videos.each(function () {
                observer.observe(this);
                });

                gsap.set(".curtain-content", { x: "-100%" });

                // 动画：从左向右展开
                gsap.to(".curtain-content", {
                x: "0%", // 移动到原始位置
                duration: 2, // 动画时长
                ease: "power3.out", // 缓动效果
                });

                let video = $('#text-video')[0];
                let animationTriggered = false; 

                $( video).on('timeupdate', function () {
                const progress = (video.currentTime / video.duration) * 100;

                if (progress >= 70 && !animationTriggered) {
                    animationTriggered = true; 
                    $('.sec-12-mk').addClass('textFadeIn');
