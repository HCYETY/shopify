  <script>
    document.addEventListener("DOMContentLoaded", function () {
      const BASE_URL = "https://membership.creality.com";
      let uploadedImageUrl = "";
      const DISCOUNT_CODE = "1PB138HMX2T3";
      const isMobile = window.innerWidth <= 900;

      // 
      function validateForm() {
        let isValid = true;

        // Email validation
        const email = document.getElementById("email");
        const emailError = document.getElementById("email-error");
        if (!email.value) {
          emailError.textContent = "Email is required";
          emailError.style.display = "block";
          isValid = false;
        } else if (!/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email.value)) {
          emailError.textContent = "Please enter a valid email address";
          emailError.style.display = "block";
          isValid = false;
        } else if (email.value.length > 100) {
          emailError.textContent =
            "Exceeds the 100 character maximum length limit";
          emailError.style.display = "block";
          isValid = false;
        }
        const machineType = document.getElementById("machine-type");
        const machineTypeError = document.getElementById("machine-type-error");
        if (machineType.dataset.value === "default") {
          machineTypeError.textContent = "Please Select your machine type";

          machineTypeError.style.display = "block";
          isValid = false;
        }
        const whyUpgrade = document.getElementById("why-upgrade");
        const whyUpgradeError = document.getElementById("why-upgrade-error");

        if (!whyUpgrade.value) {
          whyUpgradeError.textContent = "Please input your upgrade reason";
          whyUpgradeError.style.display = "block";
          isValid = false;
        } else if (whyUpgrade.value.length > 200) {
          whyUpgradeError.textContent =
            "Exceeds the 200 character maximum length limit";
          whyUpgradeError.style.display = "block";
          isValid = false;
        }
        const printerBrand = document.getElementById("printer-brand");
        const printerBrandError = document.getElementById(
          "printer-brand-error"
        );
        if (printerBrand.dataset.value === "default") {
          printerBrandError.textContent =
            "Please input your previous 3D printer brand";
          printerBrandError.style.display = "block";
          isValid = false;
        }
        // Upgrade to validation

        const upgradeTo = document.getElementById("upgrade-to");
        const upgradeToError = document.getElementById("upgrade-to-error");

        if (upgradeTo.dataset.value === "default") {
          upgradeToError.textContent = "Please select your upgrade model";
          upgradeToError.style.display = "block";
          isValid = false;
        }
        // Purchase proof validation
        const purchaseProof = document.getElementById("purchase-proof");
        const purchaseProofError = document.getElementById(
          "purchase-proof-error"
        );
        if (!purchaseProof.value) {
          purchaseProofError.textContent = "Please upload purchase proof image";
          purchaseProofError.style.display = "block";
          isValid = false;
        }

        return isValid;
      }

      function hideError(input) {
        const errorElement = document.getElementById(input.id + "-error");
        errorElement.style.display = "none";
      }
      function checkMaxLength(input, maxLength) {
        const lengthError = document.getElementById(input.id + "-length-error");
        if (input.value.length > maxLength) {
          lengthError.style.display = "block";
        } else {
          lengthError.style.display = "none";
        }
      }

      //
      document.getElementById("email").addEventListener("input", function () {
        hideError(this);
      });
      document
        .getElementById("machine-type")
        .addEventListener("input", function () {
          hideError(this);
        });
      document
        .getElementById("why-upgrade")

        .addEventListener("input", function () {
          hideError(this);
        });
      // 
      const purchaseProof = document.getElementById("purchase-proof");
      purchaseProof.addEventListener("input", (e) => validateFile(e.target));
      const uploadArea = document.querySelector(".upload-area");
      uploadArea.addEventListener("click", function () {
        document.getElementById("purchase-proof").click();
      });
      //
      async function validateFile(input) {
        const file = input.files[0];
        const purchaseProofError = document.getElementById(
          "purchase-proof-error"
        );

        if (!file) return;

        // 
        const validTypes = ["image/jpeg", "image/png", "image/webp"];
        if (!validTypes.includes(file.type)) {
          purchaseProofError.textContent =
            "Please upload files in JPEG, PNG or WebP format only";
          purchaseProofError.style.display = "block";
          input.value = "";
          return;
        }

        //  (10MB)
        const maxSize = 10 * 1024 * 1024; // 10MB
        if (file.size > maxSize) {
          purchaseProofError.textContent = "File size must not exceed 10MB";
          purchaseProofError.style.display = "block";
          input.value = "";
          return;
        }
        // 
        document.getElementById("upload-icon").style.display = "none";
        const preview = document.getElementById("preview-image");
        preview.style.display = "block";

        if (file.type.includes("image")) {
          const reader = new FileReader();
          reader.onload = function (e) {
            preview.src = e.target.result;
          };
          document.querySelector(".upload-icon-wrap").style.display = "none";
          reader.readAsDataURL(file);
        } else {
          preview.src = ""; // 
        }
        // 
        hideError(input);

        try {
          const formData = new FormData();
          formData.append("file", file);

          const response = await fetch(
            `${BASE_URL}/personal-center/membershipPersonalCenterTradein/uploadImage`,
            {
              method: "POST",
              body: formData,
            }
          );
          const res = await response.json();
          if (res.code === 200) {
            uploadedImageUrl = res.data;
          }
        } catch (error) {
          showTooltip(
            "fail",
            "Failed to upload file. Please check your file and try again."
          );
        }
      }

      // 
      function throttle(func, delay) {
        let timeoutId;
        let lastExecTime = 0;
        return function (...args) {
          const currentTime = Date.now();

          if (currentTime - lastExecTime > delay) {
            func.apply(this, args);
            lastExecTime = currentTime;
          } else {
            clearTimeout(timeoutId);
            timeoutId = setTimeout(() => {
              func.apply(this, args);
              lastExecTime = Date.now();
            }, delay - (currentTime - lastExecTime));
          }
        };
      }
      // 
      function clearForm() {
        // 
        document.getElementById("email").value = "";
        document.getElementById("why-upgrade").value = "";

        // 
        document.getElementById("machine-type").textContent =
          "Select Your Machine Type";
        document.getElementById("machine-type").dataset.value = "default";
        document.getElementById("machine-type").classList.remove("has-value");

        document.getElementById("printer-brand").textContent =
          "Select Your current model-Brand";
        document.getElementById("printer-brand").dataset.value = "default";
        document.getElementById("printer-brand").classList.remove("has-value");

        document.getElementById("upgrade-to").textContent =
          "Select Your Upgrade Machine Type";
        document.getElementById("upgrade-to").dataset.value = "default";
        document.getElementById("upgrade-to").classList.remove("has-value");

        // 
        document.getElementById("purchase-proof").value = "";
        document.getElementById("upload-icon").style.display = "block";
        document.getElementById("preview-image").style.display = "none";
        document.querySelector(".upload-icon-wrap").style.display = "block";
      }
      // 
      function collectFormData() {
        const formData = {
          email: document.getElementById("email").value,
          oldModels:
            document.getElementById("machine-type").dataset.value || "",
          oldModelsType:
            document.getElementById("printer-brand").dataset.value || "",
          upgradeTheModel:
            document.querySelector(".selected-value").dataset.value || "",
          pictureUrl: uploadedImageUrl,
          reason: document.getElementById("why-upgrade").value,
          siteCode: "US",
          platform: "falcon",
          discountCode: DISCOUNT_CODE,
          approvalStatus: "NO_NEED_APPROVAL",
        };

        return formData;
      }
      // 
      function toggleFormPopUp() {
        const formPopUp = document.querySelector(".form-pop-up");
        const formContainer = document.querySelector(".form-container");
        const discountCode = document.getElementById("discount-code");
        const isFormVisible = formContainer.style.display !== "none";

        formContainer.style.display = isFormVisible ? "none" : "flex";
        formPopUp.style.display = isFormVisible ? "flex" : "none";

        if (isFormVisible) {
          discountCode.textContent = DISCOUNT_CODE;
        }
      }
      // 
      function copyToClipboard(text) {
        navigator.clipboard
          .writeText(text)
          .then(() => {
            showTooltip("success", "Copied to clipboard!");
          })
          .catch(() => {
            // 
            const textArea = document.createElement("textarea");
            textArea.value = text;
            document.body.appendChild(textArea);
            textArea.select();
            document.execCommand("copy");
            document.body.removeChild(textArea);
            showTooltip("success", "Copied to clipboard!");
          });
      }

      //
      const copyImg = document.querySelector(".copy-img");
      copyImg.addEventListener("click", function (event) {
        const discountCode =
          document.getElementById("discount-code").textContent;
        copyToClipboard(discountCode);
      });

      //
      let isSubmitting = false;
      const submitBtn = document.getElementById("submit-btn");

      const throttledHandleSubmit = throttle(async function (event) {
        event.preventDefault();
        const isvalidata = validateForm();
        const formData = collectFormData();
        if (isvalidata) {
          if (uploadedImageUrl) {
            let isSubmitting = true;
            submitBtn.disabled = true;
            submitBtn.textContent = "Submitting...";
            try {
              const response = await fetch(
                `${BASE_URL}/personal-center/membershipPersonalCenterTradein/saveApply`,
                {
                  method: "POST",
                  headers: {
                    "Content-Type": "application/json",
                  },
                  body: JSON.stringify(formData),
                }
              );
              const res = await response.json();
              if (res.code === 200) {
                showTooltip(
                  "success",
                  "Thanks! Your application is in.We'll review it within 24 hours and send you the upgrade offer via email."
                );
                setTimeout(() => {
                  clearForm();
                }, 10);
                toggleFormPopUp();
              }
            } catch (err) {
              showTooltip("fail", "Failed to submit form. Please try again.");
            } finally {
              uploadedImageUrl = "";
              isSubmitting = false; // 
              submitBtn.disabled = false; // 
              submitBtn.textContent = "Submit"; // 
            }
          } else {
            showTooltip("fail", "Failed to submit form. Please try again.");
          }
        }
      }, 1000);
      submitBtn.addEventListener("click", throttledHandleSubmit);

      function showTooltip(type, customText) {
        const tooltipWrap = document.querySelector(".tooltip-wrap");

        const message =
          customText ||
          (type === "success"
            ? "Thanks! Your application is in.We'll review it within 24 hours and send you the upgrade offer via email."
            : "Sorry, something went wrong. Please try again.");

        const iconSrc =
          type === "success"
            ? "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/AntD-check-circle.png?v=1758000872"
            : "https://cdn.shopify.com/s/files/1/0799/8714/7049/files/merchant_del.png?v=1758000880";

        tooltipWrap.innerHTML = `
    <div class="tooltip-icon">
      <img src="${iconSrc}" alt="" width="100%" height="100%" />
    </div>
    <div class="tooltip">${message}</div>
  `;

        tooltipWrap.classList.remove("hide");
        tooltipWrap.classList.add("show");

        setTimeout(() => {
          tooltipWrap.classList.remove("show");
          tooltipWrap.classList.add("hide");
        }, 3000);
      }

      // 
      const selectTrigger = document.querySelectorAll(".select-trigger");
      const selectOptions = document.querySelectorAll(".select-options");
      selectTrigger.forEach((item, index) =>
        item.addEventListener("click", function () {
          // 
          selectOptions.forEach((option, i) => {
            if (i !== index) {
              option.style.display = "none";
            }
          });

          // 
          selectOptions[index].style.display =
            selectOptions[index].style.display === "block" ? "none" : "block";
        })
      );

      document.querySelectorAll(".option").forEach((option) => {
        option.addEventListener("click", function () {
          const selectedValue =
            this.closest(".custom-select").querySelector(".selected-value");
          selectedValue.textContent = this.textContent;
          selectedValue.dataset.value = this.dataset.value;

          if (selectedValue.dataset.value !== "default") {
            hideError(selectedValue);
          }

          selectedValue.classList.add("has-value");
          this.closest(".select-options").style.display = "none";
        });
      });

      document.addEventListener("click", function (e) {
        if (!e.target.closest(".custom-select")) {
          selectOptions.forEach((option) => {
            option.style.display = "none";
          });
        }
      });

      // upgradenow

      document
        .querySelectorAll(".save-big-btn-wrap, .banner-btn-wrap")
        .forEach((btn) => {
          btn.addEventListener("click", () => {
            document.querySelector(".enroll-machine-form").scrollIntoView({
              behavior: "smooth",
            });
          });
        });
    });
  </script>
