<script>
gsap.registerPlugin(ScrollToPlugin);
gsap.registerPlugin(ScrollTrigger)
const delay = function (timer) {
  return new Promise((res, rej) => {
    setTimeout(() => {
      res()
    }, timer)
  })
}


let diaLoginit = function () {
  $('[data-open]').click(function (e) {
    e.preventDefault();
    let dialog_name = $(this).data('open')
    $(`.${dialog_name}`).fadeIn()
    $('html').addClass('scroll-no')
  })
  $('.close').click(function () {
    $(this).parents('.dialog').fadeOut();
    $('html').removeClass('scroll-no')
  })
}

let tarGetinit = function () {
  $('[data-target]').click(function (e) {
    e.preventDefault();
    // $('.page-pagination li').removeClass('active')
    // $(this).addClass('active')
    let target = $(this).data('target')
    gsap.to(window, {
      duration: 0.5,
      scrollTo: {
        y: target
      }
    });
  })
  let pagePagination = $(".page-pagination")

  $(window).scroll(function () {
    if (docWidth > 960) {
      var scrollPos = $(window).scrollTop();
      let docHeight = document.documentElement.clientHeight

      $(".page-pagination li").each(function (key) {
        var target = $(this).data("target");
        var targetPos = $(target).offset().top;
        var targetHeight = $(target).outerHeight();

        // 检查元素是否在视图中
        if (scrollPos >= (targetPos - docHeight * (1 - 0.3))) {
          pagePagination.fadeIn();
          $('.page-pagination li').removeClass('active')
          $(this).addClass("active"); // 如果在视图中，添加"active"类
        } else if (key === 0) {
          pagePagination.fadeOut()
        }
      });
    }
  });
}

var fadeInListen = function () {
  var fadeIn = $('.main-contain').find('.fadein');

  fadeIn.each(function () {
    let element = this;
    let {
      scrub,
      reverse,
      duration,
      actions,
      fn
    } = $(this).data()

    gsap.set(element, {
      opacity: 0,
      y: 30
    });
    let toggleActions = "play none none none";
    let start = $(this).data('start') || 70;
    duration ? '' : duration = 0.8
    let tl = gsap.to(element, {
      opacity: 1,
      y: 0,
      duration
    });
    if (reverse) {
      toggleActions = "play none none reverse"
    }

    ScrollTrigger.create({
      animation: tl,
      trigger: element,
      toggleActions,
      start: `top ${start}%`, // 调整触发点，视情况而定
      onEnter: function () {

      },
      once: true // 只触发一次
    });
  });

  var delayFadeIn = $('.delay-fadein');
  delayFadeIn.each(function () {
    let element = this;
    let {
      parent,
      el,
      scrub,
      reverse,
      duration,
      actions,
      fn
    } = $(this).data()
    let parentEl = $(parent);
    parentEl.length ? '' : parentEl = $(element)
    let delayEl = [];
    if (el) {
      el.split(',').forEach((selector) => delayEl.push(...parentEl.find(
        selector)))
    } else {
      delayEl = parentEl.children();
    }

    let toggleActions = "play none none none";
    scrub ? scrub = true : scrub = false;

    duration ? '' : duration = 0.8
    console.log(duration)
    let start = $(this).data('start') || 70;
    let stagger = $(this).data('delay') || 0.3;


    gsap.set(delayEl, {
      opacity: 0,
      y: 30
    });

    let tl = gsap.timeline();
    tl.to(delayEl, {
      stagger,
      opacity: 1,
      y: 0,
      duration
    })
    if (fn) {
      console.log(fn)
      tl = k1c_tl[fn](tl)
    }
    if (reverse) {
      toggleActions = "play none none reverse"
      gsap.set(element, {
        opacity: 0,
        y: 30
      });
      let t2 = gsap.to(element, {
        opacity: 1,
        y: 0,
        duration
      })
      ScrollTrigger.create({
        animation: t2,
        toggleActions,
        trigger: element,
        scrub,
        start: `top ${start}%`, // 调整触发点，视情况而定
      });
    }
    actions ? toggleActions = actions : ''


    ScrollTrigger.create({
      animation: tl,
      toggleActions,
      trigger: element,
      scrub,
      start: `top ${start}%`, // 调整触发点，视情况而定
      // marker: true
      // once: true // 只触发一次
    });
  });

}


