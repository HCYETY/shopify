<script defer>
  document.addEventListener('DOMContentLoaded', function () {
    // 获取所有expand-icon元素
    const expandIcons = document.querySelectorAll('.footer .footer-menu .expand-icon');

    // 为每个expand-icon添加点击事件
    expandIcons.forEach(function (icon) {
      icon.addEventListener('click', function () {
        // 获取最近的父元素div.footer-menu-item，然后查找其中的menu-list
        const parentItem = this.closest('.footer-menu-item');
        const menuList = parentItem.querySelector('.menu-list, ul:not(.menu-list)');

        // 切换menu-list的显示状态和图标旋转状态
        if (menuList) {
          menuList.classList.toggle('active');
          this.classList.toggle('active');
        }
      });
    });
  });
</script>

