{% style %}
  .footer {
    background-color: #fff;
    padding-bottom: calc(80px + var(--product-nav-bar--height, 0px));
    padding-top: 80px;
  }

  .footer .container {
    padding: 0;
  }

  .footer .footer-menu {
    display: flex;
    gap: 40px;
    margin-bottom: 80px;
  }

  .footer .footer-menu .footer-menu-item {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
  }

  .footer .footer-menu .contact-list {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  .footer .footer-menu .contact-item {
    display: grid;
    grid-template-columns: 20px 1fr;
    grid-template-rows: 20px 1fr;
    gap: 4px;
  }

  .footer .footer-menu .contact-item svg {
    fill: none;
    stroke: #1e1e1e;
  }

  .footer .footer-menu .contact-item .contact-icon {
    grid-column: 1;
    grid-row: 1;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .footer .footer-menu .contact-item .contact-icon-phone svg {
    width: 20px;
    height: 20px;
  }

  .footer .footer-menu .contact-item .contact-icon-address svg,
  .footer .footer-menu .contact-item .contact-icon-email svg {
    width: 16px;
    height: 16px;
  }

  .footer .footer-menu .contact-item .contact-title {
    grid-column: 2;
    grid-row: 1;
    color: #1e1e1e;
    font-family: Montserrat;
    font-size: 14px;
    font-style: normal;
    font-weight: 400;
    line-height: 150%;
  }

  .footer .footer-menu .contact-item .contact-content {
    grid-column: 2;
    grid-row: 2;
    color: #8d9095;
    font-family: Montserrat;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 150%;
  }

  .footer .footer-links {
    display: flex;
    gap: 16px;
    justify-content: space-between;
  }

  .footer .footer-links-left {
  }

  .footer .footer-links-left img {
    max-height: 24px;
  }

  .footer .footer-links-left a:after {
    display: none;
  }

  .footer .footer-links-right {
    display: flex;
    column-gap: 20px;
    row-gap: 8px;
    flex-wrap: wrap;
  }

  .footer .footer-links-right a {
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .footer .footer-links-right svg {
    height: 20px;
    width: 20px;
    fill: none;
    stroke: none;
  }

  .footer .footer-links-payIcon {
    display: flex;
    gap: 8px;
    flex-wrap: wrap;
  }

  .footer .footer-links-payIcon span {
    display: flex;
    align-items: center;
  }

  .footer .footer-links-payIcon svg {
    height: 22px;
    width: auto;
    fill: none;
    stroke: initial;
  }

  .footer .footer-desc {
    border-top: 1px solid #eaebed;
    margin-top: 24px;
    padding-top: 24px;
    display: flex;
    gap: 16px;
    justify-content: space-between;
  }

  .footer .footer-desc-copyright span {
    color: #8d9095;
    font-family: Montserrat;
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 150%;
  }

  .footer .footer-desc .cooperation-tag {
    padding-left: 24px;
  }

  .footer p {
    color: #1e1e1e;
    font-size: 14px;
    margin-bottom: 12px;
    text-transform: uppercase;
  }

  .footer ul {
    display: grid;
    row-gap: 12px;
  }

  .footer a {
    color: #8d9095;
    font-size: 12px;
    position: relative;
    width: -webkit-fit-content;
    width: -moz-fit-content;
    width: fit-content;
  }

  .footer a,
  .footer a:after {
    display: block;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
  }

  .footer a:after {
    background: #1e1e1e;
    bottom: 0;
    content: '';
    height: 1px;
    position: absolute;
    right: 0;
    width: 0;
  }

  .footer a:hover {
    color: #1e1e1e;
  }

  .footer a:hover:after {
    left: 0;
    right: auto;
    width: 100%;
  }

  @media (min-width: 1920px) {
    .container {
      margin: 0 auto !important;
      max-width: 62.5vw !important;
      width: 62.5vw !important;
      padding: 0;
    }
  }
  @media (min-width: 901px) and (max-width: 1920px) {
    .container {
      padding: 0;
    }
  }
  @media (max-width: 900px) {
    .footer .container {
      -webkit-box-sizing: border-box;
      box-sizing: border-box;
      max-width: 100% !important;
      padding-left: 6.4vw;
      padding-right: 6.4vw;
      width: 100% !important;
    }
  }

  @media (min-width: 901px) {
    .lg\:hidden {
      display: none;
    }
  }

  @media (max-width: 900px) {
    /* mobile specific styles */
    .footer {
      padding-top: 6.4vw;
    }
    .footer ul {
      display: none;
    }
    .footer ul.active {
      display: grid;
    }
    .footer .footer-menu {
      flex-direction: column;
      gap: 0;
      margin-bottom: 12.8vw;
    }
    .footer .footer-menu .footer-menu-item {
      border-bottom: 0.267vw solid #e5e5e5;
      padding: 4.267vw 0;
    }
    .footer a {
      font-size: 3.733vw;
    }
    .footer .footer-menu .footer-menu-item svg {
      width: 6.4vw;
      height: 6.4vw;
      stroke: none;
      transition: transform 0.3s ease;
    }
    .footer .footer-menu .footer-menu-item svg.active {
      transform: rotate(45deg);
    }
    .footer .footer-menu .footer-menu-item p {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin: 0;
      font-weight: 500;
      line-height: 150%;
    }
    .footer .footer-menu .footer-menu-item ul {
      margin-top: 3.2vw;
    }
    .footer .footer-links {
      flex-direction: column;
    }
    .footer .footer-desc {
      flex-direction: column-reverse;
      margin-top: 5.333vw;
      padding-top: 5.333vw;
    }
  }

  @media (min-width: 1920px) {
    .footer {
      padding-bottom: calc(4.167vw + var(--product-nav-bar--height, 0px));
      padding-top: 4.167vw;
    }
    .footer .footer-menu {
      gap: 2.083vw;
      margin-bottom: 4.167vw;
    }
    .footer .footer-menu .contact-list {
      gap: 0.625vw;
    }
    .footer .footer-menu .contact-item {
      grid-template-columns: 1.042vw 1fr;
      grid-template-rows: 1.042vw 1fr;
      gap: 0.208vw;
    }
    .footer .footer-menu .contact-item .contact-icon-phone svg {
      width: 1.042vw;
      height: 1.042vw;
    }
    .footer .footer-menu .contact-item .contact-icon-address svg,
    .footer .footer-menu .contact-item .contact-icon-email svg {
      width: 0.833vw;
      height: 0.833vw;
    }
    .footer .footer-menu .contact-item .contact-title {
      font-size: 0.729vw;
    }
    .footer .footer-menu .contact-item .contact-content {
      font-size: 0.625vw;
    }
    .footer .footer-links {
      gap: 0.833vw;
    }
    .footer .footer-links-left img {
      max-height: 1.25vw;
    }
    .footer .footer-links-right {
      column-gap: 1.042vw;
      row-gap: 0.417vw;
    }
    .footer .footer-links-right svg {
      height: 1.042vw;
      width: 1.042vw;
    }
    .footer .footer-links-payIcon {
      gap: 0.417vw;
    }
    .footer .footer-links-payIcon svg {
      height: 1.146vw;
    }
    .footer .footer-desc {
      border-top: 0.052vw solid #eaebed;
      margin-top: 1.25vw;
      padding-top: 1.25vw;
      gap: 0.833vw;
    }
    .footer .footer-desc-copyright span {
      font-size: 0.625vw;
    }
    .footer .footer-desc .cooperation-tag {
      padding-left: 1.25vw;
    }
    .footer p {
      font-size: 0.729vw;
      margin-bottom: 0.625vw;
    }
    .footer ul {
      row-gap: 0.625vw;
    }
    .footer a {
      font-size: 0.625vw;
    }
    .footer a:after {
      height: 0.052vw;
    }
  }
{% endstyle %}

