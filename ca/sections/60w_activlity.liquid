<script>

        let docWidth = document.documentElement.clientWidth;
        // 每秒更新活动倒计时
        function getActivityCountdown(countryCode) {
            // 创建时区偏移量对象
            var timeZoneOffsets = {
                'us': -4, // 美国东部时间偏移量为 -4 小时
                'uk': 1, // 英国时间偏移量为 +1 小时
                'es': 2, // 西班牙时间偏移量为 +2 小时
                'eu': 2 // 欧洲中部时间偏移量为 +2 小时
            };

            // 获取当前时间和活动开始时间
            var currentTime = new Date();
            var activityStartTime = new Date(currentTime.getFullYear(), 5, 15, 22, 0, 0); // 3 月 26 日凌晨 0 点

            // 根据国家代码获取时区偏移量
            var timeZoneOffset = timeZoneOffsets[countryCode];

            // 根据时区偏移量调整活动开始时间
            activityStartTime.setHours(activityStartTime.getHours() + timeZoneOffset);

            // 计算活动开始倒计时
            var countdown = Math.max(0, activityStartTime - currentTime);
            var days = Math.floor(countdown / (1000 * 60 * 60 * 24));
            var hours = Math.floor((countdown % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((countdown % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((countdown % (1000 * 60)) / 1000);
            console.log(hours)

            return {
                days,
                hours: ("0" + hours).slice(-2),
                minutes: ("0" + minutes).slice(-2),
                seconds: ("0" + seconds).slice(-2),
            };
        }

        // 更新活动倒计时
        function updateCountdown() {
            var countryCode = 'us'; // 替换为你希望的国家代码
