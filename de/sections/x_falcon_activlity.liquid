          station = 'eu'
        }else if(headHref.indexOf('uk') != -1){
          station = 'uk'
        }


        window.myMessage = function (text) {
            var div = document.createElement('div');
            div.className = 'my-message';
            div.innerHTML = `<div style="position: fixed;transform: translate(-50%,-50%);left: 50%;top: 50%;padding: 20px;font-size: 14px;color: #fff;background-color: rgba(0,0,0,.5);z-index: 999;word-break: keep-all;white-space: nowrap;">${text}</div>`;
            document.body.appendChild(div);
            setTimeout(function () {
                div.style.transition = 'opacity .3s ease';
                div.style.opacity = 0;
            }, 1200)
            setTimeout(function () {
                div.remove();
            }, 2200)
        }

        function copy(text) {
            var textareaEl = document.createElement('textarea');
            textareaEl.setAttribute('readonly', 'readonly'); // 防止手机上弹出软键盘
            // input.setAttribute('value', text);
            textareaEl.value = text;
            document.body.appendChild(textareaEl);
            // input.setSelectionRange(0, 9999);
            textareaEl.select();
            var res = document.execCommand('copy');
            document.body.removeChild(textareaEl);
            console.log("复制成功");
            return res;
        }

        const delay = ms => new Promise(res => setTimeout(res, ms));
        gsap.registerPlugin(ScrollToPlugin);
        gsap.registerPlugin(ScrollTrigger)


        $('.radar-wave').toggleClass('radar-animate')

        $('.radar-btn .wave6').on('animationend', async function () {
            $(this).parent().find('.radar-wave').toggleClass('radar-animate');
            await delay(300)
            $(this).parent().find('.radar-wave').toggleClass('radar-animate');
        })

        function giftOpened(day) {
            for (let i = 1; i < day; i++) {
                $(`.day${i}`).hide()
                $(`.day${i}`).parent('.gift_item').find('.opened_gift').css({
                    opacity: .7
                });
            }
        }
        function ajaxPromise(url, options) {
            return new Promise(function(resolve, reject) {
                $.ajax(url, options)
                    .done(function(data) {
                        resolve(data); // 请求成功时，调用 resolve 并传递响应数据
                    })
                    .fail(function(xhr, status, error) {
                        reject(error); // 请求失败时，调用 reject 并传递错误信息
                    });
            });
        }
       // 更新活动倒计时

      // 每秒更新活动倒计时
        function getActivityCountdown(activityStartTime_) {
          // 创建时区偏移量对象
          

          // 获取当前时间和活动开始时间
          var currentTime = new Date();
          var activityStartTime = activityStartTime_


          // 计算活动开始倒计时
          var countdown = Math.max(0, activityStartTime - currentTime);
          var hours = Math.floor(countdown / (1000 * 60 * 60));
          var minutes = Math.floor((countdown % (1000 * 60 * 60)) / (1000 * 60));
          var seconds = Math.floor((countdown % (1000 * 60)) / 1000);

          // 格式化倒计时字符串
          var countdownString = ("0" + hours).slice(-2) + "h" + ("0" + minutes).slice(-2) + "m" + ("0" + seconds).slice(-2) + "s";

          return countdownString;
      }

        function updateCountdown() {
            const timeZoneOffsets = {
              'us': 7, // 美国东部时间偏移量为 -7 小时
              'uk': 1, // 英国时间偏移量为 +1 小时
              'es': 2, // 西班牙时间偏移量为 +2 小时
              'eu': 1 // 欧洲中部时间偏移量为 +2 小时
            };
          
            var currentTime = new Date();
            const activityStartTime = new Date(Date.UTC(2024, 2, 26, 0, 0, 0)); // UTC 时间的 3 月 26 日凌晨 0 点
            // const activityStartTime = new Date(Date.UTC(2024, 2, 22, 14, 59, 0)); // UTC 时间的 3 月 26 日凌晨 0 点
            
            // const activityStartTime =  new Date(currentTime.getFullYear(), 2, 26, 0, 0, 0); // 3 月 26 日凌晨 0 点
            var timeZoneOffset = timeZoneOffsets[station];

            activityStartTime.setHours(activityStartTime.getHours() + timeZoneOffset);

            var timer = setInterval(function(){
              var currentTime = new Date();
              if (currentTime >= activityStartTime) {
                  clearInterval(timer); // 停止计时器
                  return getUserData();
              }
              
              let countdownResult = getActivityCountdown(activityStartTime);
              let timeText = "Countdown to Event Start " + countdownResult;
              $('.tips_div').text(timeText)
            }, 1000); // 每秒更新一次            

            // 检查是否到达活动开始时间，如果是则停止计时器

        }
        updateCountdown()
        //更新文本信息
        function updateText(day_num,gift_content,gift_code, already_day_num){
          // 初始化弹窗信息
          if (day_num == 2 || day_num == 4 || day_num == 6) {
              $('.gift_dialog .title').text('Sign in successfully!')
              $('.gift_dialog .content').hide()
          } else {    //有礼物盒子的情况
              $('.gift_dialog .title').text('congratulations！')

              $('.gift_dialog .gift_text').text(gift_content)
              $('.gift_dialog .gift_code').text(gift_code)

              let gift_img = $(`.day${day_num}`).parent('.gift_item').find('.opened_gift').clone()
              if(day_num === 7) {
                  gift_img = $('.gift7_icon').clone()
              }
              $('.gift_dialog .gift_parent').find('img').remove();
              $('.gift_dialog .gift_parent').append(gift_img)
          }
          let sign_text = `Already signed up for ${already_day_num} days, sign up again for ${7 - already_day_num} days to receive all gifts.`
          if (day_num === 7) {
              sign_text = `Already signed up for 7 days, Congrats on all the gifts!`
          }
          $('.gift_dialog .sign_text').text(sign_text)
          $('.tips_div').text(sign_text)
        }

        async function getUserData() { //发送请求获取用户数据          
            if(!is_login) {  //是否登录
                return $('.log_text').show()
            }
            
