<script>
gsap.registerPlugin(ScrollToPlugin);
gsap.registerPlugin(ScrollTrigger)
const delay = function (timer) {
  return new Promise((res, rej) => {
    setTimeout(() => {
      res()
    }, timer)
  })
}

const docWidth = document.documentElement.clientWidth;
const docHeight = document.documentElement.clientHeight;
const vw = docWidth / 100;
const vh = docHeight / 100;
let spaceBetween = 1.3 * vw;

let spaceNum = docWidth > 960 ? 0.012 : 0.05

let swiperSetting = {
  spaceBetween: docWidth * spaceNum,
  loopAdditionalSlides: 1, // 设置额外的滑块数量，以实现平滑过渡
  centeredSlides: true,
  slidesPerGroup: 1,
  slidesPerView: 3,
  loop: true,
  navigation: {
    nextEl: '.next-btn',
    prevEl: '.prev-btn',
  },
  on:{
    transitionEnd: function(){
      let activeSlide = this.slides[this.activeIndex];
      let tabId = activeSlide.getAttribute('name');
      if(docWidth<950){
        let prevTabsId = this.slides[this.previousIndex].getAttribute('name');
        $(`#${prevTabsId}`)[0].nextElementSibling.classList.remove('tab-item-active');
        $(`#${tabId}`)[0].nextElementSibling.classList.add('tab-item-active');
      }
      $(`#${tabId}`)[0].checked = true;
    },
  },
}
let mySwiper;
function resetSwiperConfig(newOptions, docW) {
  if (mySwiper) {
    mySwiper.forEach(v=>{
      if(docW < 950){
        [v.params.slidesPerView, v.params.centeredSlides] = [1, false];
      }else{
        [v.params.slidesPerView, v.params.centeredSlides] = [3, true];
      }
      v.update();
    })
  }else{
    if(docW < 950) [newOptions.slidesPerView, newOptions.centeredSlides] = [1, false];
    mySwiper = new Swiper('.sec-swiper', newOptions); 
  }
}
function debounce(func, wait) {
  let timeout;
  return function(...args) {
      const context = this;
      clearTimeout(timeout);
      timeout = setTimeout(function() {
          func.apply(context, args);
      }, wait);
  };
}

const handleResize = debounce(function() {
    resetSwiperConfig(null, document.documentElement.clientWidth);
}, 300);

window.addEventListener('resize', handleResize);

$('.tabs').click((e)=>{
  if(docWidth < 960 ){
    e.preventDefault();
    return
  }
  let clickTabsIndex = e.currentTarget.getAttribute('index');
  let tabsIndex = e.target.getAttribute('index');
  tabsIndex && mySwiper[clickTabsIndex].slideTo(tabsIndex, 300, false);
  e.preventDefault();
})

let tarGetinit = function () {
  $('[data-target]').click(function (e) {
    e.preventDefault();
    let target = $(this).data('target')
    gsap.to(window, {
      duration: 0.5,
      scrollTo: {
        y: target
      }
    });
  })
  let pagePagination = $(".page-pagination")

  $(window).scroll(function () {
    if (docWidth > 960) {
      var scrollPos = $(window).scrollTop();
      let docHeight = document.documentElement.clientHeight

      $(".page-pagination li").each(function (key) {
        var target = $(this).data("target");
        var targetPos = $(target).offset().top;
        var targetHeight = $(target).outerHeight();

        // 检查元素是否在视图中
        if (scrollPos >= (targetPos - docHeight * (1 - 0.3))) {
          pagePagination.fadeIn();
          $('.page-pagination li').removeClass('active')
          $(this).addClass("active"); // 如果在视图中，添加"active"类
        } else if (key === 0) {
          pagePagination.fadeOut()
        }
      });
    }
  });
}

var fadeInListen = function () {
  var fadeIn = $('.main-contain').find('.fadein');

  fadeIn.each(function () {
    let element = this;
    let {
      scrub,
      reverse,
      duration,
      actions,
      fn
    } = $(this).data()

    gsap.set(element, {
      opacity: 0,
      y: 30
    });
    let toggleActions = "play none none none";
    let start = $(this).data('start') || 70;
    duration ? '' : duration = 0.8
    let tl = gsap.to(element, {
      opacity: 1,
      y: 0,
      duration
    });
    if (reverse) {
      toggleActions = "play none none reverse"
    }

    ScrollTrigger.create({
      animation: tl,
      trigger: element,
      toggleActions,
      start: `top ${start}%`, // 调整触发点，视情况而定
      onEnter: function () {

      },
      once: true // 只触发一次
    });
  });

  var delayFadeIn = $('.delay-fadein');
  delayFadeIn.each(function () {
    let element = this;
    let {
      parent,
      el,
      scrub,
      reverse,
      duration,
      actions,
      fn
    } = $(this).data()
    let parentEl = $(parent);
    parentEl.length ? '' : parentEl = $(element)
    let delayEl = [];
    if (el) {
      el.split(',').forEach((selector) => delayEl.push(...parentEl.find(
        selector)))
    } else {
      delayEl = parentEl.children();
    }

    let toggleActions = "play none none none";
    scrub ? scrub = true : scrub = false;

    duration ? '' : duration = 0.8
    console.log(duration)
    let start = $(this).data('start') || 70;
    let stagger = $(this).data('delay') || 0.3;


    gsap.set(delayEl, {
      opacity: 0,
      y: 30
    });

    let tl = gsap.timeline();
    tl.to(delayEl, {
      stagger,
      opacity: 1,
      y: 0,
      duration
    })
    if (fn) {
      console.log(fn)
      tl = k1c_tl[fn](tl)
    }
    if (reverse) {
      toggleActions = "play none none reverse"
      gsap.set(element, {
        opacity: 0,
        y: 30
      });
      let t2 = gsap.to(element, {
        opacity: 1,
        y: 0,
        duration
      })
      ScrollTrigger.create({
        animation: t2,
        toggleActions,
        trigger: element,
        scrub,
        start: `top ${start}%`, // 调整触发点，视情况而定
      });
    }
    actions ? toggleActions = actions : ''


    ScrollTrigger.create({
      animation: tl,
      toggleActions,
      trigger: element,
      scrub,
      start: `top ${start}%`, // 调整触发点，视情况而定
      // marker: true
      // once: true // 只触发一次
    });
  });

}

const swiperWrapper = $('.section11 .s11-swiper .swiper-wrapper,.section12 .s12-swiper .swiper-wrapper');

// 鼠标悬停时暂停动画
swiperWrapper.hover(
  function () {
    $(this).css('animation-play-state', 'paused');
  },
  function () {
    $(this).css('animation-play-state', 'running');
  }
);

// 淡入淡出 初始化
fadeInListen();
tarGetinit() //跳转初始化
resetSwiperConfig(swiperSetting, docWidth);

    // <!-- lazy load -->
    const picture_observer = lozad('.fit_picture,.lazy-video', {
        rootMargin: '1500px 0px', // syntax similar to that of CSS Margin
        load: function (el) {
            // console.log(el)
            if (el.tagName === 'PICTURE' || el.tagName === 'IMG') {
                let image = el.tagName === 'IMG' ? el : el.querySelector('img');
                if (el.tagName === 'IMG') image = el;

                image.onload = function () {
                    this.classList.add('loaded')
                }
                el.querySelectorAll('source').forEach(source => {
                    let srcset = source.getAttribute('data-srcset');
                    srcset ? source.setAttribute('srcset', srcset) : ''
                });
            } else if (el.tagName === 'VIDEO') {
                el.onloadeddata = function () {
                    console.log('loaded...')
                    this.classList.add('loaded');
                };
                const poster = el.getAttribute('data-poster');
                if (poster) el.setAttribute('poster', poster);

                el.querySelectorAll('source').forEach(source => {
                    const src = source.getAttribute('data-src');
                    if (src) {
                        source.setAttribute('src', src);
                        el.load()
                    }
                });
            }
        }
    });
    picture_observer.observe();
</script>

