<script type="module">
  $(function() {
  const isMobile = document.documentElement.clientWidth <= 768;
  // 监听滚动
  function fadeInListen() {
    var fadeIn = $('.container-box').find('.fadein');
    fadeIn.each(function () {
      let element = this;
      let {
        scrub,
        reverse,
        duration,
        actions,
        fn
      } = $(this).data()

      gsap.set(element, {
        opacity: 0,
        y: 20
      });
      let toggleActions = "play none none none";
      let start = $(this).data('start') || 70;
      duration ? '' : duration = 0.8
      let tl = gsap.to(element, {
        opacity: 1,
        y: 0,
        duration
      });
      if (reverse) {
        toggleActions = "play none none reverse"
      }

      ScrollTrigger.create({
        animation: tl,
        trigger: element,
        toggleActions,
        start: `top ${start}%`, // 调整触发点，视情况而定
        onEnter: function () {

        },
        once: true // 只触发一次
      });
    });
  }
  fadeInListen(); 
  // 轮播一
  new Swiper('#app-{{section.id}} .swiper-products .products', {
    slidesPerView: isMobile ? 2 : "auto",
    spaceBetween: isMobile ? 10 :  20, 
    autoplay: {
      delay: 3000
    },
    loop: true
  });

  $(".form-content input[type='text']").each(function(i, item) {
    if(!$(this).hasClass("expected-time")) {
      let num;
      if(document.documentElement.clientWidth <= 768) {
        num = 30
      } else {
        num = 70
      }
      var width =  $(this).prev().width() 
      console.log(width,num,'num')
      $(this).css("padding-left", width + num + 'px')
    }
  })

  function setImg(nodes) {
    nodes.each(function(i, item) {
      $(this).click(function() {
        const id = $(this).attr("id");
        nodes.each(function(i,item) {
          $(this).parent().find(`label .box`).css("background-image", "url()")
        })
        $(this).parent().find(`label[for=${id}] .box`).css("background-image", "url('https://cdn.shoplazza.com/9d4fb627031f50ec273e583cf546189f.png')")
      })
    })
    nodes.eq(0).trigger("click");
  }
  const socialMedias =  $(".form-content input[name='socialMedia']");
  const socialbeen =  $(".form-content input[name='socialbeen']");
  const socialtype =  $(".form-content input[name='socialtype']");

  setImg(socialMedias);
  setImg(socialbeen);
  setImg(socialtype);


  $('#info-form input').on('input', function() {
    // 清除当前输入框的错误信息
    $(this).siblings('.error-message').remove();

    var $input = $(this);
    var value = $input.val().trim();

    // 检查是否为必填项
    if ($input.is('[required]') && value === '') {
        $input.after('<div class="error-message">This is a required field</div>');
    }

    // 特别处理邮箱验证
    if ($input.attr('type') === 'email' && value !== '') {
        var emailRegex = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
        if (!emailRegex.test(value)) {
            $input.after('<div class="error-message">Please enter a valid email address</div>');
        }
    }
  });

  // 表单提交
  $('.submit-btn').on('click', function(e) {
    e.preventDefault(); // 防止表单默认提交

    // 清除之前的错误信息
    $('.error-message').remove();

    var isValid = true;

    // 遍历每个必填项进行验证
    $('#info-form input[type="text"], #info-form input[type="radio"]:checked').each(function() {
        var $input = $(this);
        if ($input.val().trim() === '') {
            isValid = false;
            $input.after('<div class="error-message">This is a required field</div>');
        }
    });

    // 邮箱验证
    var email = $('#email').val();
    if (email) {
      var emailRegex = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
      if (!emailRegex.test(email)) {
        isValid = false;
        $('#email').after('<div class="error-message">Please enter a valid email address</div>');
      }
    }

    if (isValid) {
        // 构造表单数据
        var formData = {
          'name': $('#your-name').val(),
          'phoneNumber': $('#phone-number').val(),
          'email': $('#email').val(),
          'socialMediaLink': $('#media-url').val(), 
          'socialMedia': $('input[name="socialMedia"]:checked').val(),
          'isBuyTicket': Boolean($('input[name="socialbeen"]:checked').val()),
          'ticketType': $('input[name="socialtype"]:checked').val(),
          'arrivalTime': $('#expected-time').val(),
          'station': 'au',
          'channel': 'shopify' 
        };

        // 调用API
        $.ajax({
            url: 'https://bsplugins.creality.com/openapi/dtc/activity/20240819/form',
            type: 'POST',
            contentType: 'application/json',
            data: JSON.stringify(formData),
            success: function(response) {
              if(response.code == 200) {
                $.toast({
                  type: 'success',
                  duration: 3000,
                  text: 'Submitted successfully!',
                  icon: 'success',
                  position: 'mid-center',  // 中间位置
                  loader: true,        // Change it to false to disable loader
                  loaderBg: '#9EC600'  // To change the background
                });
                $('#your-name').val('');
                $('#phone-number').val('');
                $('#email').val('');
                $('#media-url').val('');
                $('#expected-time').val('');
              } 
              if (response.code == 500) {
                $.toast({
                  text: res.msg,
                  icon: 'error',
                  heading: 'error',
                  position: 'mid-center',  // 中间位置
                  duration: 3000
                });
              }
              if (response.code == 3001) {
                $.toast({
                  heading: 'error',
                  text: 'This email has already been submitted',
                  icon: 'error',
                  position: 'mid-center',  // 中间位置
                  duration: 3000
                });
              }
              
            },
            error: function(error) {
                alert('Form submission failed, please try again later.');
            }
        });
    }
  });
})

