                "prevEl": ".section4 .s3_swiper_2_2 .navigation-wrap .prev-btn"
            }
        });
        var swiper2 = new Swiper('.section7 .s7_swiper', {
            allowTouchMove: false, // Disable dragging
            direction: 'vertical',
        });
        $('.section7 .swiper-tabitem').click(function () {
            var index = $(this).index();
            $(this).addClass('tab-active').siblings().removeClass('tab-active')

            swiper2.slideTo(index); // Change to the specified slide index
