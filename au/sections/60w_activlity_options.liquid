<script>
        let station = 'us'
        let headHref = window.location.href.split('.')[0]
        if(headHref.indexOf('eu') != -1){
          station = 'eu'
        }else if(headHref.indexOf('uk') != -1){
          station = 'uk'
        }

        let docWidth = document.documentElement.clientWidth;
        // 
        function getActivityCountdown(countryCode) {
            // 
            const timeZoneOffsets = {
              'us': -7, //  -7 
              'uk': 1, //  +1 
              'es': 2, //  +2 
              'eu': 1 //  +2 
            };

            // 
            var currentTime = new Date();
            var activityStartTime = new Date(currentTime.getFullYear(), 5, 15, 22, 0, 0); // 3  26  0 

            // 
            var timeZoneOffset = timeZoneOffsets[countryCode];

            // 
            activityStartTime.setHours(activityStartTime.getHours() + timeZoneOffset);

            // 
            var countdown = Math.max(0, activityStartTime - currentTime);
            var days = Math.floor(countdown / (1000 * 60 * 60 * 24));
            var hours = Math.floor((countdown % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((countdown % (1000 * 60 * 60)) / (1000 * 60));
            var seconds = Math.floor((countdown % (1000 * 60)) / 1000);
            console.log(hours)

            return {
                days,
                hours: ("0" + hours).slice(-2),
                minutes: ("0" + minutes).slice(-2),
                seconds: ("0" + seconds).slice(-2),
            };
        }

        // 
        function updateCountdown() {
            var countryCode = station; // 
            var countdownResult = getActivityCountdown(countryCode);
            // console.log(" " + countdownResult); // 
            $('.time-days').text(countdownResult.days)
            $('.time-hours').text(countdownResult.hours)
            $('.time-minutes').text(countdownResult.minutes)
            $('.time-seconds').text(countdownResult.seconds)



            // 
            var currentTime = new Date();
            var activityStartTime = new Date(currentTime.getFullYear(), 5, 15, 22, 0, 0); // 3  26  0 
            if (currentTime >= activityStartTime) {
                clearInterval(timer); // 
                window.location.reload(); // 
            }
        }
        // 
        var timer = setInterval(updateCountdown, 1000); // 
        var swiper1 = new Swiper('.section4 .s3_swiper_1', {
            allowTouchMove: false, // Disable dragging
            direction: 'vertical',
        });
