            var index = $(this).index();
            $('.progress-wrap .progress-line').css('transform', `translateX(${(index) * 100}%)`)

            swiper1.slideTo(index); // Change to the specified slide index
        })

        let spaceNum = docWidth < 960 ? 0.02 : 0.005;
        var mySwiper2 = new Swiper('.section4 .s3_swiper_2_1 .swiper-container', {
            allowTouchMove: true, // Disable dragging
            slidesPerView: 'auto',
            spaceBetween: docWidth * spaceNum,
            on: {
                resize: function () { // 
                    docWidth = document.documentElement.clientWidth;
                    this.params.spaceBetween = docWidth * spaceNum; // 
                    this.update(); // Swiper
                }
            },
            loop: true,
            // autoplay: {
            //     delay: 2500
            // },
            navigation: {
