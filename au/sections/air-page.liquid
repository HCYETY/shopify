  .left-daohang {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
        -ms-flex-direction: column;
            flex-direction: column;
    position: fixed;
    top: 50%;
    -webkit-transform: translateY(-50%);
        -ms-transform: translateY(-50%);
            transform: translateY(-50%);
    /* transition: all 0.3s ease  */
    color: #5f6368;
    left: 1vw;
    list-style: none;
    padding-left: 0;
    margin: 0;
    font-size: 0.7vw;
    visibility: hidden;
    opacity: 0;
    -webkit-transition: opacity 0.3s ease, visibility 0.3s ease;
    transition: opacity 0.3s ease, visibility 0.3s ease;
    z-index: 99;
  }
  .left-daohang ul {
    list-style: none;
  }
  /* 白色背景下的导航项样式 */
  .left-daohang li {
    position: relative;
    padding-left: 20px;
    margin: 8px 0;
    cursor: pointer;
  }
  /* 默认灰色竖条 */
  .left-daohang li::before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    width: 3px;
    background: #ddd;
    border-radius: 2px;
    -webkit-transition: all 0.3s ease;
    transition: all 0.3s ease;
  }
  /* 白色背景下的文字颜色 */
  .left-daohang li p {
    color: #5f6368;
    -webkit-transition: color 0.3s ease;
    transition: color 0.3s ease;
  }
  /* 白色背景下的悬停效果 */
  .left-daohang li:hover p {
    color: #333;
  }
  /* 白色背景下的选中状态 */
  .left-daohang li.active::before {
    background: #333;
  }
  .left-daohang li.active p {
    color: #333;
    font-weight: bold;
  }
  /* 当处于黑色背景区域时 */
  .dark-section .left-daohang li::before {
    background: rgba(255, 255, 255, 0.3);
