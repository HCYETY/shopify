<script>
    document.addEventListener('DOMContentLoaded', function () {
        // 获取所有需要自动播放的视频元素
        const videos = document.querySelectorAll('video');
        videos.forEach(video => {
            video.muted = true;
            video.loop = true;
            video.playsInline = true;
            video.dataset.played = 'false';
        });

        // 视频视口检测逻辑
        let isScrolling;
        window.addEventListener('scroll', function () {
            window.clearTimeout(isScrolling);
            isScrolling = setTimeout(checkVideosInViewport, 100);
        });
        checkVideosInViewport();

        function checkVideosInViewport() {
            videos.forEach(video => {
                const isInView = isElementInViewport(video);
                if (isInView && video.dataset.played === 'false') {
                    video.play().then(() => video.dataset.played = 'true');
                } else if (!isInView && !video.paused) {
                    video.pause();
                    video.dataset.played = 'false';
                    video.currentTime = 0;
                }
            });
        }

        function isElementInViewport(el) {
            const rect = el.getBoundingClientRect();
            const windowHeight = window.innerHeight || document.documentElement.clientHeight;
            const visibleHeight = Math.min(rect.bottom, windowHeight) - Math.max(rect.top, 0);
            return visibleHeight / rect.height >= 0.2;
        }

        const sidebar = document.querySelector('.left-daohang');
        const secondSection = document.querySelector('#second-partial');
        let secondSectionBottom = 0;

        let isSidebarTransitioning = false; // 侧边栏是否正在过渡
        let isSidebarVisible = false; // 侧边栏当前是否可见

        // 更新模块边界
        function updateSectionBounds() {
            if (secondSection) {
                secondSectionBottom = secondSection.offsetTop + secondSection.offsetHeight;
            }
        }
        window.addEventListener('load', updateSectionBounds);
        window.addEventListener('resize', updateSectionBounds);

        // 滚动监听函数
        function handleScroll() {
            if (isSidebarTransitioning) return;
            const scrollPosition = window.pageYOffset;
            const secondSectionTop = secondSection ? secondSection.offsetTop : 0;

            const shouldBeVisible = scrollPosition >= secondSectionTop;


            if (shouldBeVisible !== isSidebarVisible) {
                isSidebarVisible = shouldBeVisible;
                isSidebarTransitioning = true; // 锁定状态

                if (shouldBeVisible) {
                    // 显示侧边栏
                    sidebar.style.visibility = 'visible';
                    sidebar.style.opacity = 1;

                    // 过渡完成后解锁
                    setTimeout(() => {
                        isSidebarTransitioning = false;
                    }, 300);
                } else {
                    // 隐藏侧边栏
                    sidebar.style.opacity = 0;

                    // 过渡完成后完全隐藏
                    setTimeout(() => {
                        sidebar.style.visibility = 'hidden';
                        isSidebarTransitioning = false;
                    }, 300);
                }
            }

            // 深色模式和高亮逻辑（保持不变）
            updateSectionBounds();
            if (secondSection && scrollPosition >= secondSectionTop && scrollPosition <= secondSectionBottom) {
                sidebar.classList.add('dark-mode');
            } else {
                sidebar.classList.remove('dark-mode');
            }
            highlightNav();
        }

        // 平滑滚动逻辑
        document.querySelectorAll('.left-daohang li[link-url]').forEach(item => {
            item.addEventListener('click', function (e) {
                e.preventDefault();
                const targetId = this.getAttribute('link-url');
                const targetElement = document.querySelector(targetId);
                if (targetElement) {
                    window.scrollTo({
                        top: targetElement.offsetTop - 100,
                        behavior: 'smooth'
                    });
                    history.pushState(null, null, targetId);
                }
            });
        });

        // 导航项高亮逻辑
        const sections = document.querySelectorAll('[id$="-partial"]');
        const navItems = document.querySelectorAll('.left-daohang li[link-url]');
        function highlightNav() {
            let currentId = '';
            const scrollPosition = window.pageYOffset + 150;
            sections.forEach(section => {
                if (scrollPosition >= section.offsetTop) {
                    currentId = section.id;
                }
            });
            navItems.forEach(item => {
                item.classList.toggle('active', item.getAttribute('link-url') === `#${currentId}`);
            });
        }

        // 绑定滚动事件（仅一次）
        window.addEventListener('scroll', handleScroll);
        window.addEventListener('load', handleScroll); // 初始化时触发
    });
</script>
