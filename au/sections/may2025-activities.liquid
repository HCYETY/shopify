      // sec4
      $(function () {
          const sec4Swiper = new Swiper('.sec4-list-swiper', {
              direction: 'horizontal', // 垂直切换选项
              slidesPerView: 2,
              spaceBetween: 5,
              loop: true,
              breakpoints: {
  
                  900: {
                      slidesPerView: 4,
                    
                  }
              },
          })
          $(".sec4-list-button-prev").on("click", function (e) {
              e.preventDefault();
              sec4Swiper.slidePrev();
          });
  
          $(".sec4-list-button-next").on("click", function (e) {
              e.preventDefault();
              sec4Swiper.slideNext();
          });
      })
      $(function () {
          $(".sec1-list1 .sec1-list1-item").on("click", function () {
              $(`
                #${$(this).attr("class").split(" ")[0]}`).get(0).scrollIntoView({ behavior: 'smooth' });
    })
    $(".product-quick-shop-buy-now").on("click",async function(){
        const id = $(this).attr("data-id");
        $(this).addClass("is-loading");
        $.ajax({
          type: "POST",
          url: "/cart/clear.js",
          data: {},
          dataType: "json",
          success: function() { 
            $.ajax({
              type: "POST",
              url: "/cart/add.js",
              data: {
                quantity: 1,
                id
              },
              dataType: "json",
              success: function() { 
                window.location.href = "/checkout";
                $(this).removeClass("is-loading");
              }
            });
          }
        });
      })
      })
 
  </script>
