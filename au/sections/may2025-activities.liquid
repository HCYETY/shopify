  <script>
      // sec1
      $(function () {
          const sec1Swiper = new Swiper('.sec1-list2-swiper', {
              direction: 'horizontal', // 垂直切换选项
              slidesPerView: 3,
              loop: true,
              breakpoints: {
  
                  900: {
                      slidesPerView: 4,
                  }
              },
          })
          $(".sec1-list2-button-prev").on("click", function (e) {
              e.preventDefault();
              sec1Swiper.slidePrev();
          });
  
          $(".sec1-list2-button-next").on("click", function (e) {
              e.preventDefault();
              sec1Swiper.slideNext();
          });
      });
