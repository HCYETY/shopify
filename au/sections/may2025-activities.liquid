        })
        $(".sec2-prev").on("click", function (e) {
            e.preventDefault();
            sec2Swiper.slidePrev();
        });

        $(".sec2-next").on("click", function (e) {
            e.preventDefault();
            sec2Swiper.slideNext();
        });
    })
    // sec2-2
    $(function () {
